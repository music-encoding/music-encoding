<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
  elementFormDefault="qualified"
  xmlns:xlink="http://www.w3.org/1999/xlink">
  <xs:include schemaLocation="datatypes.xsd"/>
  <xs:import namespace="http://www.w3.org/1999/xlink"
    schemaLocation="xlink.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"
    schemaLocation="xml.xsd"/>
  <!-- ************************************************************** -->
  <!-- * Schematron constraints                                     * -->
  <!-- ************************************************************** -->
  <!-- ************************************************************** -->
  <!-- * MEI datatypes                                              * -->
  <!-- ************************************************************** -->
  <!-- ************************************************************** -->
  <!-- * Attribute classes                                          * -->
  <!-- ************************************************************** -->
  <xs:attributeGroup name="att.alignment">
    <xs:annotation>
      <xs:documentation>Temporal alignment attributes. This class of
        attributes is unavailable unless the meiLinkAlign module is
        included.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="when" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>indicates the point of occurrence along a
          timeline. Its value must be the ID of a &lt;when&gt;
          element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.altsym">
    <xs:annotation>
      <xs:documentation>Pointers to user-defined
        symbols</xs:documentation>
    </xs:annotation>
    <xs:attribute name="altsym" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a user-defined
          symbol. It must contain an ID of a &lt;symboldef&gt;
          element elsewhere in the document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.bibl">
    <xs:annotation>
      <xs:documentation>Bibliographic attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="analog"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.color">
    <xs:annotation>
      <xs:documentation>Color attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="color" type="data.COLOR">
      <xs:annotation>
        <xs:documentation>used to indicate visual appearance. Do not
          confuse this with the musical term 'color' as used in
          pre-CMN notation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.common.anl">
    <xs:annotation>
      <xs:documentation>Common analytical attributes. When the
        meiLinkAlign module is used, the when attribute is also a
        member of this attribute class.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="copyof" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>points to an element of which the current
          element is a copy.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="corresp" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>used to point to other elements that
          correspond to this one in a generic
          fashion.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="next" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>used to point to the next event(s) in a
          user-defined collection.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="prev" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>points to the previous event(s) in a
          user-defined collection.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sameas" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>points to an element that is the same as the
          current element but is not a literal copy of the current
          element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="synch" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>points to elements that are synchronous with
          the current element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup ref="att.alignment"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.common">
    <xs:annotation>
      <xs:documentation>Common attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute ref="xml:id"/>
    <xs:attribute name="n" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>provides a label (name or number) for an
          element. While its value need not be unique, it is required
          to be a single token.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.controlevent">
    <xs:annotation>
      <xs:documentation>Attributes shared by events which rely on
        other events for their existence. For example, a slur/phrase
        marking must be drawn between or over a group of notes. The
        slur is therefore a control event.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="layer" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the layer within which the control
          event is active.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="plist" type="data.PLIST">
      <xs:annotation>
        <xs:documentation>may contain ID references or xPointer
          fragment identifiers that identify logical events that
          participate in the control event, such as notes under a
          phrase mark.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="staff" use="required"
      type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the staff within which a control
          event is active.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp" type="data.BEAT">
      <xs:annotation>
        <xs:documentation>encodes the onset time of a musical feature
          in terms of musical time, i.e., beats[.fractional beat
          part]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.ges" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>records the onset time in pulses per quarter
          note (ppq, MusicXML divisions, or MIDI clicks) since the
          start of the measure.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.real" type="data.ISOTIME">
      <xs:annotation>
        <xs:documentation>records the onset time as a real-time
          offset, i.e., HH:MM:SS.ss, since the beginning of the
          file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.coordinated">
    <xs:annotation>
      <xs:documentation>This attribute class records the position of
        the entity within a two-dimensional coordinate
        system.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="ulx" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>indicates the upper-left corner x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="uly" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>indicates the upper-left corner y
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lrx" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>indicates the lower-right corner x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lry" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>indicates the lower-left corner x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.crit">
    <xs:annotation>
      <xs:documentation>Attributes common to all elements representing
        variant readings</xs:documentation>
    </xs:annotation>
    <xs:attribute name="cause" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>classifies the cause for the variant
          reading, according to any appropriate typology of possible
          origins.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="hand" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>signifies the hand responsible for a
          particular reading in a witness. The value of hand should
          correspond to the ID of a &lt;hand&gt; element given
          elsewhere in the document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="resp" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>The editor(s) responsible for asserting a
          particular reading in a witness may be recorded in the resp
          attribute. The value of resp must point to one or more of
          the identifiers declared in the document header, associated
          with a person asserted as responsible for some aspect of the
          text's creation, transcription, editing, or
          encoding.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="seq" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>provides an integer indicating the position
          of this reading in a sequence, when there is reason to
          presume a sequence of the variant
          readings.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="source" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>contains a list of one or more pointers
          indicating the sources which attest to a given reading. Each
          value should correspond to the ID of a
          &lt;source&gt; element located in the document
          header.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.date">
    <xs:annotation>
      <xs:documentation>Attributes common to dates</xs:documentation>
    </xs:annotation>
    <xs:attribute name="calendar" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>indicates the system or calendar to which
          the date belongs, for example, Gregorian, Julian, Roman,
          Mosaic, Revolutionary, Islamic, etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="cert" type="data.CERTAINTY">
      <xs:annotation>
        <xs:documentation>indicates the degree of precision to be
          attributed to the date.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="from" type="data.ISODATE">
      <xs:annotation>
        <xs:documentation>contains the starting point of a date
          range.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="notafter" type="data.ISODATE">
      <xs:annotation>
        <xs:documentation>contains an upper boundary for an uncertain
          date.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="notbefore" type="data.ISODATE">
      <xs:annotation>
        <xs:documentation>contains a lower boundary for an uncertain
          date.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="reg" type="data.ISODATE">
      <xs:annotation>
        <xs:documentation>gives the value of a date in standard ISO
          form.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="to" type="data.ISODATE">
      <xs:annotation>
        <xs:documentation>contains the end point of a date
          range.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.edit">
    <xs:annotation>
      <xs:documentation>Attributes describing the nature of an encoded
        scholarly intervention or interpretation</xs:documentation>
    </xs:annotation>
    <xs:attribute name="cert" type="data.CERTAINTY">
      <xs:annotation>
        <xs:documentation>signifies the degree of certainty associated
          with the intervention or interpretation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="evidence" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>indicates the nature of the evidence
          supporting the reliability or accuracy of the intervention
          or interpretation. Suggested values include: 'internal',
          'external', 'conjecture'.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="resp" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>records the editor(s) responsible for
          asserting a particular reading in a witness. Its value must
          point to one or more of the identifiers declared in the
          document header, associated with a person asserted as
          responsible for some aspect of the text's creation,
          transcription, editing, or encoding.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="source" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>contains a list of one or more pointers
          indicating the sources which support the given
          reading.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.event">
    <xs:annotation>
      <xs:documentation>Attributes that apply to all written events,
        e.g., beam, beatrpt, chord, note, etc.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="layer" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the layer within which the event
          is active.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="staff" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the staff within which the event
          is active.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp" type="data.BEAT">
      <xs:annotation>
        <xs:documentation>encodes the onset time of the event in terms
          of musical time, i.e., beats[.fractional beat
          part]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.ges" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>records the onset time in pulses per quarter
          note (ppq, MusicXML divisions, or MIDI clicks) since the
          start of the measure.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.real" type="data.ISOTIME">
      <xs:annotation>
        <xs:documentation>records the onset time as a real-time
          offset, i.e., in HH:MM:SS.ss format, since the beginning of
          the file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.facsimile">
    <xs:annotation>
      <xs:documentation>Facsimile references. This attribute class is
        unavailable unless the meiFacsimile module is
        available.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="facs" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>permits the current element to reference a
          facsimile image or image zone which corresponds to
          it.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.link.common">
    <xs:annotation>
      <xs:documentation>Attributes common to all linking
        elements</xs:documentation>
    </xs:annotation>
    <xs:attribute ref="xlink:actuate"/>
    <xs:attribute ref="xlink:role"/>
    <xs:attribute ref="xlink:show"/>
    <xs:attribute name="targettype" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>in contrast with the role attribute, allows
          the target resource to be characterized, using any
          convenient classification scheme or
          typology.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute ref="xlink:title"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.link.external">
    <xs:annotation>
      <xs:documentation>Linking attributes for external referencing
        elements</xs:documentation>
    </xs:annotation>
    <xs:attribute name="entityref" type="xs:ENTITY">
      <xs:annotation>
        <xs:documentation>uses a previously-declared entity to
          identify an external electronic object.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fragment" type="data.FRAGMENT">
      <xs:annotation>
        <xs:documentation>permits identification of one or more
          sub-parts within the object being pointed
          to.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute ref="xlink:href"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.link.internal">
    <xs:annotation>
      <xs:documentation>Linking attributes for internal referencing
        elements</xs:documentation>
    </xs:annotation>
    <xs:attribute name="plist" type="data.PLIST">
      <xs:annotation>
        <xs:documentation>contains an ID reference for each of the
          participants in a collection of elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.midi.event">
    <xs:annotation>
      <xs:documentation>Attributes common to MIDI
        events</xs:documentation>
    </xs:annotation>
    <xs:attribute name="layer" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the layer to which the MIDI event
          applies.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="staff" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the staff to which the MIDI event
          applies.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp" type="data.BEAT">
      <xs:annotation>
        <xs:documentation>encodes the onset time of the MIDI event in
          terms of musical time, i.e., beats[.fractional beat
          part]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.name">
    <xs:annotation>
      <xs:documentation>Attributes shared by names</xs:documentation>
    </xs:annotation>
    <xs:attribute name="authority">
      <xs:annotation>
        <xs:documentation>contains the name of the list from which a
          controlled value is taken, such as the Thesaurus of
          Geographic Names (TGN), etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>used to record a value which serves as a
          primary key in an external database.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="reg">
      <xs:annotation>
        <xs:documentation>used to record a regularized form of the
          name.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.placement">
    <xs:annotation>
      <xs:documentation>Attributes capturing placement
        information</xs:documentation>
    </xs:annotation>
    <xs:attribute name="place" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>captures the placement of the item with
          respect to the staff with which it is
          associated.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.startendid">
    <xs:annotation>
      <xs:documentation>Attributes recording the IDs of a sequence of
        elements to which the current element is
        associated.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="startid" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>holds a reference to the first element in
          the sequence.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="endid" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>indicates the final element in the
          sequence.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.lang">
    <xs:annotation>
      <xs:documentation>Language attributes common to text
        elements</xs:documentation>
    </xs:annotation>
    <xs:attribute ref="xml:lang"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.trans">
    <xs:annotation>
      <xs:documentation>Attributes for elements encoding authorial or
        scribal intervention when transcribing manuscript or similar
        sources</xs:documentation>
    </xs:annotation>
    <xs:attribute name="hand" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>signifies the hand of the agent which made
          the intervention. Its value must be the ID of a
          &lt;hand&gt; element declared in the
          header.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="seq" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>used to assign a sequence number related to
          the order in which the encoded features carrying this
          attribute are believed to have occurred.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.typed">
    <xs:annotation>
      <xs:documentation>Attributes which can be used to classify or
        subclassify elements</xs:documentation>
    </xs:annotation>
    <xs:attribute name="type" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>characterizes the element in some sense,
          using any convenient classification scheme or
          typology.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="subtype" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>provide any sub-classification for the
          element, additional to that given by its type
          attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.typography">
    <xs:annotation>
      <xs:documentation>Typographical attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="fontfam" type="data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>contains the name of a
          font-family.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fontname" type="data.FONTNAME">
      <xs:annotation>
        <xs:documentation>records the name of the
          font.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fontsize" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>indicates the size of the font in printers'
          points, i.e., 1/72ths of an inch.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fontstyle" type="data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>records the style of the font, i.e, italic,
          oblique, or normal.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fontweight" type="data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>used to indicate bold
          type.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset.ho">
    <xs:annotation>
      <xs:documentation>Horizontal offset
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="ho" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records a horizontal adjustment of the
          entity's programmatically-determinded location in terms of
          interline distance; that is, units of 1/2 the distance
          between adjacent staves.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset.to">
    <xs:annotation>
      <xs:documentation>Horizontal offset attributes specified in
        terms of time</xs:documentation>
    </xs:annotation>
    <xs:attribute name="to">
      <xs:annotation>
        <xs:documentation>records a timestamp adjustment of the
          entity's programmatically-determined
          location.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset.vo">
    <xs:annotation>
      <xs:documentation>Vertical offset attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="vo" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records the vertical adjustment of the
          entity's programmatically-determined location in terms of
          interline distance; that is, units of 1/2 the distance
          between adjacent staves.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset">
    <xs:annotation>
      <xs:documentation>Visual offset attributes. Some items may have
        their location recorded in terms of offsets from their
        programmatically-determined location. The ho attribute records
        the horizontal offset while vo records the vertical. The to
        attribute holds a timestamp offset, the most common use of
        which is as an alternative to the ho
        attribute.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.visualoffset.ho"/>
    <xs:attributeGroup ref="att.visualoffset.to"/>
    <xs:attributeGroup ref="att.visualoffset.vo"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset2.ho">
    <xs:annotation>
      <xs:documentation>Horizontal offset requiring a pair of
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="startho" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records the horizontal adjustment of the
          entity's programmatically-determined start
          point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="endho" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records the horizontal adjustment of the
          entity's programmatically-determined end
          point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset2.to">
    <xs:annotation>
      <xs:documentation>Horizontal offset attributes requiring a pair
        of attributes specified in terms of time</xs:documentation>
    </xs:annotation>
    <xs:attribute name="startto">
      <xs:annotation>
        <xs:documentation>records a timestamp adjustment of the
          entity's programmatically-determined start
          point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="endto">
      <xs:annotation>
        <xs:documentation>records a timestamp adjustment of the
          entity's programmatically-determined end
          point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset2.vo">
    <xs:annotation>
      <xs:documentation>Vertical offset attributes requiring a pair of
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="startvo" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records a vertical adjustment of the
          entity's programmatically-determined start
          point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="endvo" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records a vertical adjustment of the
          entity's programmatically-determined end
          point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.visualoffset2">
    <xs:annotation>
      <xs:documentation>Visual offset attributes. Some items may have
        their location recorded in terms of pairs of offsets from
        their programmatically-determined location. The startho and
        endho attributes record the horizontal offsets of the start
        and end points of the item, respectively. Similarly, the
        startvo and endvo attributes record the vertical offsets of
        the start and end points of the item. The startto and endto
        attributes hold timestamp offsets, the most common use of
        which is as alternatives to the ho
        attributes.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.visualoffset2.ho"/>
    <xs:attributeGroup ref="att.visualoffset2.to"/>
    <xs:attributeGroup ref="att.visualoffset2.vo"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.xy">
    <xs:annotation>
      <xs:documentation>Output coordinate attributes. Some elements
        may have their exact rendered *output* coordinates recorded. x
        and y attributes indicate where to place the rendered output.
        Recording the coordinates of a feature in a facsimile requires
        the use of the facs attribute.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="x" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the optional x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the optional y
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.xy.required">
    <xs:annotation>
      <xs:documentation>Required output coordinate attributes. Some
        elements must have their exact rendered *output* coordinates
        recorded. x and y attributes indicate where to place the
        rendered output. Recording the coordinates of a feature in a
        facsimile requires the use of the facs
        attribute.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="x" use="required" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the required x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y" use="required" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the required y
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.xy2">
    <xs:annotation>
      <xs:documentation>Output coordinate attributes. Some elements
        may need 2 coordinate pairs to record their rendered *output*
        coordinates.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="x2" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the optional 2nd x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y2" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the optional 2nd y
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.xy2.required">
    <xs:annotation>
      <xs:documentation>Required output coordinate attributes. Some
        elements must have 2 coordinate pairs to record their output
        coordinates.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="x2" use="required" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the required 2nd x
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y2" use="required" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the required 2nd y
          coordinate.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!--
    Declare attributes in each musical domain, e.g. analytical,
    gestural, and visual, for each element. This documents which domain
    an attribute operates in and makes it easier to modify the attributes
    in a given domain independently of the others.
  -->
  <!-- ************************************************************** -->
  <!-- ************************************************************** -->
  <!-- * Attributes for barline                                     * -->
  <xs:attributeGroup name="att.anl.barline">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.barline">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="tstamp">
      <xs:annotation>
        <xs:documentation>encodes the onset time of the barline in
          terms of musical time, i.e., beats[.fractional beat
          part]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.barline">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="complete">
      <xs:annotation>
        <xs:documentation>allows the encoding of whether the measure
          preceding this barline matches the prevailing meter: a value
          of 'c' indicates a metrically complete measure, 'i'
          indicates a measure with not enough beats, while 'o' is for
          measures with too many beats.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="c"/>
          <xs:enumeration value="i"/>
          <xs:enumeration value="o"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="control" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether or not the barline is
          "controlling"; that is, if it indicates a point of alignment
          across all the parts. Barlines within a score are usually
          controlling' that is, they "line up". Barlines within parts
          may or may not be controlling.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rend" type="data.BARRENDITION">
      <xs:annotation>
        <xs:documentation>records the appearance of the
          barline.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.barline">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attribute name="barplace" type="data.BARPLACE">
      <xs:annotation>
        <xs:documentation>records the location of the
          barline.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="taktplace" type="data.STAFFLOC">
      <xs:annotation>
        <xs:documentation>If takt barlines are to be used, then the
          taktplace attribute may be used to denote the staff location
          of the shortened barline. The location may include staff
          lines, spaces, and the spaces directly above and below the
          staff. The value ranges between 0 (just below the staff) to
          2 * number of staff lines (directly above the staff). For
          example, on a 5-line staff the lines would be numbered
          1,3,5,7, and 9 while the spaces would be numbered
          0,2,4,6,8,10. For example, a value of '9' puts the barline
          through the top line of a 5-line staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="width" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>For rendering purposes, some music data
          systems rely on measure width data. The width attribute
          allows for the capture of this data in MEI for output back
          to these systems.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for clef                                        * -->
  <xs:attributeGroup name="att.anl.clef">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.clef">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="line" use="required" type="data.CLEFLINE">
      <xs:annotation>
        <xs:documentation>the staff location of the clef. The value
          must be in the range between 1 and the number of lines on
          the staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="octave.default" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>The default octave, used when the first note
          of a measure provides no oct attribute, is set by the
          octave.default attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="shape" use="required" type="data.CLEFSHAPE">
      <xs:annotation>
        <xs:documentation>decribes the clef symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dis" type="data.OCTAVE.DIS">
      <xs:annotation>
        <xs:documentation>records the amount of octave displacement to
          be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dis.place" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>records the direction of octave displacement
          to be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.clef">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for custos                                      * -->
  <xs:attributeGroup name="att.anl.custos">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributess</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.custos">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="oct" use="required" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>together with the pname attribute,
          identifies the location where the custos
          appears.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pname" use="required" type="data.PITCHNAME">
      <xs:annotation>
        <xs:documentation>in conjuction with the oct attribute,
          identifies the location where the custos
          appears.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="source" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>contains a list of one or more pointers
          indicating the sources which contain the
          custos.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="target" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>encodes the target note when its pitch
          differs from the pitch at which the custos
          appears.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.custos">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for dot                                         * -->
  <xs:attributeGroup name="att.anl.dot">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.dot">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="form">
      <xs:annotation>
        <xs:documentation>records the function of the dot; that is,
          whether it is a dot of augmentation or a dot of
          division.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="aug"/>
          <xs:enumeration value="div"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="loc" type="data.STAFFLOC">
      <xs:annotation>
        <xs:documentation>encodes the staff location of the
          dot.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.dot">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.xy"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for ending                                      * -->
  <xs:attributeGroup name="att.anl.ending">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for grpsym                                      * -->
  <xs:attributeGroup name="att.anl.grpsym">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.grpsym">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="symbol" use="required">
      <xs:annotation>
        <xs:documentation>specifies the symbol used to group a set of
          staves.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="brace"/>
          <xs:enumeration value="bracket"/>
          <xs:enumeration value="line"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.grpsym">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for interruped neume                            * -->
  <xs:attributeGroup name="att.anl.ineume">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.ineume">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="form" type="data.INEUMEFORM">
      <xs:annotation>
        <xs:documentation>provides a subclass or functional label for
          the neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="name" type="data.INEUMENAME">
      <xs:annotation>
        <xs:documentation>records a name associated with the neume
          symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.ineume">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for layer                                       * -->
  <xs:attributeGroup name="att.anl.layer">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.layer">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates if a layer should be displayed
          when the notation is rendered. A layer with this attribute
          set to 'false' should also not be included in a sounded
          rendition.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for layerdef                                    * -->
  <xs:attributeGroup name="att.ges.layerdef">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="grace" type="data.GRACE">
      <xs:annotation>
        <xs:documentation>Setting the grace attribute here provides
          the possibility of changing all the grace attribute values
          in a layer without having to individually change each
          event's grace attribute value. Each "graced" element may
          still require a value in its grace.time
          attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.layerdef.base">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="beam.group">
      <xs:annotation>
        <xs:documentation>provides an example of how beaming
          (including secondary beams) is to be performed. It can be
          used to set a default beaming pattern to be used when no
          beaming is indicated at the event level. beam.group must
          contain a comma-separated list of time values that add up to
          a measure, e.g., in 4/4 time '4,4,4,4' indicates each
          quarter note worth of shorter notes would be beamed
          together. Parentheses can be used to indicate sub-groupings
          of secondary beams. For example, '(4.,4.,4.)' in 9/8 meter
          indicates one outer beam per measure with secondary beams
          broken at each dotted quarter duration, while a measure of
          16th notes in 4/4 with beam.group equal to '(4,4),(4,4)'
          will result in a primary beam covering all the notes and
          secondary beams each group of 4 notes. This beaming
          "directive" can be overridden by using &lt;beam&gt;
          elements. If neither &lt;beam&gt; elements or the
          beam.group attribute is used, then no beaming is rendered.
          Beaming can be 'turned off' by setting beam.group to an
          empty string.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="beam.rests" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether rests are allowed under a
          beam.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dur.default" type="data.DURATIONS">
      <xs:annotation>
        <xs:documentation>contains a default duration in those
          situations when the first note, rest, chord, etc. in a
          measure does not have a duration
          specified.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="octave.default" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>contains a default octave specification in
          those situations when the first note, rest, chord, etc. in a
          measure does not have an octave
          specified.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.layerdef">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for layerdef in all
        repertoires</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.log.layerdef.base"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.layerdef.base">
    <xs:annotation>
      <xs:documentation>Visual domain attributes for layerdef in the
        CMN repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="beam.rend">
      <xs:annotation>
        <xs:documentation>captures whether a beam is feathered and in
          which direction</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="acc">
            <xs:annotation>
              <xs:documentation>indicates that the secondary beams get
                progressively closer together, coming together at the
                end of the beam.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="rit">
            <xs:annotation>
              <xs:documentation>means that the secondary beams start
                at the same point and get progressively more
                distant.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="norm">
            <xs:annotation>
              <xs:documentation>indicates a return to "normal"
                beaming.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="beam.slope" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>contains the default value for the slope of
          beams.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slur.rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>provides a default value for the appearance
          of slurs when this information is not provided on the
          individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.fam" type="data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>provides a default value for the font family
          name of text (other than lyrics) when this information is
          not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.name" type="data.FONTNAME">
      <xs:annotation>
        <xs:documentation>provides a default value for the font name
          of text (other than lyrics) when this information is not
          provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.size" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>provides a default value for the font size
          of text (other than lyrics) when this information is not
          provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.style" type="data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>provides a default value for the font style
          of text (other than lyrics) when this information is not
          provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.weight" type="data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>provides a default value for the font weight
          for text (other than lyrics) when this information is not
          provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tie.rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>provides a default value for the appearance
          of ties when this information is not provided on the
          individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>provides a default indication of whether a
          layer should be displayed when the notation is rendered. A
          layer with this attribute set to 'false' should also not be
          included in a sounded rendition.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.layerdef">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.vis.layerdef.base"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for ligature                                    * -->
  <xs:attributeGroup name="att.anl.ligature">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.ligature">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="form" type="data.LIGATUREFORM">
      <xs:annotation>
        <xs:documentation>provides an indication of the function of
          the ligature.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for mensur                                      * -->
  <xs:attributeGroup name="att.anl.mensur">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.mensur">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="modusmaior">
      <xs:annotation>
        <xs:documentation>describes the maxima-long
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="modusminor">
      <xs:annotation>
        <xs:documentation>describes the long-breve
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with numbase, describes duration as a
          ratio. num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="numbase" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with num, describes duration as a
          ratio. num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="prolatio">
      <xs:annotation>
        <xs:documentation>describes the semibreve-minim
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="tempus">
      <xs:annotation>
        <xs:documentation>describes the breve-semibreve
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.mensur">
    <xs:annotation>
      <xs:documentation>Visual domain attributes. These attributes
        describe the physical appearance of the mensuration sign/time
        signature of mensural notation.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attribute name="dot" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>specifies whether a dot is to be added to
          the base symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="form">
      <xs:annotation>
        <xs:documentation>indicates whether the base symbol is written
          vertically or horizontally.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="horizontal"/>
          <xs:enumeration value="vertical"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="loc" type="data.STAFFLOC">
      <xs:annotation>
        <xs:documentation>holds the staff location of the mensuration
          sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="orient">
      <xs:annotation>
        <xs:documentation>describes the rotation or reflection of the
          base symbol.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="reversed"/>
          <xs:enumeration value="90CW"/>
          <xs:enumeration value="90CCW"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="sign" type="data.MENSURATIONSIGN">
      <xs:annotation>
        <xs:documentation>the base symbol in the mensuration sign/time
          signature of mensural notation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="size" type="data.SIZE">
      <xs:annotation>
        <xs:documentation>describes the relative size of the
          mensuration sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slash" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>indicates the number lines added to the
          mensuration sign. For example, one slash is added for what
          we now call 'alla breve'.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for midi                                        * -->
  <xs:attributeGroup name="att.anl.midi">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.midi">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="layer" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>describes the layer to which the
          &lt;midi&gt; element's children
          apply.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="staff" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>describes the staff to which the
          &lt;midi&gt; element's children
          apply.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for proport                                     * -->
  <xs:attributeGroup name="att.anl.proport">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.proport">
    <xs:annotation>
      <xs:documentation>Logical domain attributes. These attributes
        describe augmentation or diminution of the normal value of the
        notes in mensural notation.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="num" use="required"
      type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>Together, num and numbase specify the change
          as a ratio, e.g., 1:3. Num is for the first value in the
          ratio.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="numbase" use="required"
      type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>Together, num and numbase specify the change
          as a ratio, e.g., 1:3. Numbase is for the second value in
          the ratio.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for part                                        * -->
  <xs:attributeGroup name="att.anl.part">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for parts                                       * -->
  <xs:attributeGroup name="att.anl.parts">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for rdg                                         * -->
  <xs:attributeGroup name="att.anl.rdg">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.rdg">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="expan" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>contains an ordered list of IDs of
          descendant &lt;section&gt;, &lt;ending&gt;
          or &lt;rdg&gt; elements. This 'expansion list'
          indicates how the section should be expanded into its
          'through-composed' form. For example, expan="A End1 A End2"
          indicates that the section labelled 'A' comes first, then
          the ending labelled 'End1', followed by the 'A' section
          again, and finally the ending labelled
          'End2'.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for sb                                          * -->
  <!--
    doc: Analytical domain attributes: sb uses only common analysis
    attributes.
  -->
  <xs:attributeGroup name="att.anl.sb">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.sb">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="source" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>Source descriptions in the header may be
          pointed to by putting their IDs in the source attribute.
          This allows system breaks from multiple sources to be
          encoded with a single &lt;sb&gt;
          element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.sb">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="rend">
      <xs:annotation>
        <xs:documentation>indicates whether hash marks should be
          rendered between systems. See Read, p. 436, ex.
          26-3.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="hash"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for score                                       * -->
  <xs:attributeGroup name="att.anl.score">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for scoredef                                    * -->
  <xs:attributeGroup name="att.ges.scoredef">
    <xs:attribute name="grace" type="data.GRACE">
      <xs:annotation>
        <xs:documentation>Setting the grace attribute here provides
          the possibility of changing all the grace attribute values
          in a score without having to individually change each
          event's grace attribute value. Each "graced" element may
          still require a value in its grace.time
          attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="midi.channel" type="data.MIDICHANNEL">
      <xs:annotation>
        <xs:documentation>sets the default MIDI
          channel.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="midi.div" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>sets the number of MIDI ticks per quarter
          note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="midi.tempo" type="data.MIDITEMPO">
      <xs:annotation>
        <xs:documentation>sets the default MIDI
          tempo.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tempo" type="data.TEMPOVALUE">
      <xs:annotation>
        <xs:documentation>records tempo in beats (meter signature
          denominator) per minute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tune.pname" type="data.PITCHNAME">
      <xs:annotation>
        <xs:documentation>holds the pitch name, such as 'a', for a
          tuning reference pitch.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tune.Hz" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>holds a value for cycles per second, i.e.,
          Hertz, for a tuning reference pitch.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tune.temper" type="data.TEMPERAMENT">
      <xs:annotation>
        <xs:documentation>provides a label for the tuning system,
          'just', for example.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.scoredef.mensural">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for a score in the
        mensural repertoire. The values set in these attributes act as
        score-wide defaults for attributes that are not set in
        descendant elements. The tempus, prolatio, modusmaior, and
        modusminor attributes specify the relationship between the
        four principle levels of note value, i.e., the long, breve,
        semibreve and minim, in mensural notation. Modusminor
        describes the long-breve relationship, while tempus describes
        the breve-semibreve, and prolatio the semibreve-minim
        relationship, respectively. Modusmaior is for the maxima-long
        relationship. The proport.* attributes describe augmentation
        or diminution of the normal value of the notes in mensural
        notation.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="modusmaior">
      <xs:annotation>
        <xs:documentation>describes the maxima-long
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="modusminor">
      <xs:annotation>
        <xs:documentation>describes the long-breve
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with numbase, describes duration as a
          ratio. num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="numbase" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with num, describes duration as a
          ratio. num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="prolatio">
      <xs:annotation>
        <xs:documentation>describes the semibreve-minim
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="proport.num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>Together, proport.num and proport.numbase
          specify a proportional change as a ratio, e.g., 1:3.
          Proport.num is for the first value in the
          ratio.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="proport.numbase" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>Together, proport.num and proport.numbase
          specify a proportional change as a ratio, e.g., 1:3.
          Proport.numbase is for the second value in the
          ratio.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tempus">
      <xs:annotation>
        <xs:documentation>describes the breve-semibreve
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.scoredef.base">
    <xs:attribute name="beam.group">
      <xs:annotation>
        <xs:documentation>provides an example of how beaming
          (including secondary beams) is to be performed. It can be
          used to set a default beaming pattern to be used when no
          beaming is indicated at the event level. beam.group must
          contain a comma-separated list of time values that add up to
          a measure, e.g., in 4/4 time '4,4,4,4' indicates each
          quarter note worth of shorter notes would be beamed
          together. Parentheses can be used to indicate sub-groupings
          of secondary beams. For example, '(4.,4.,4.)' in 9/8 meter
          indicates one outer beam per measure with secondary beams
          broken at each dotted quarter duration, while a measure of
          16th notes in 4/4 with beam.group equal to '(4,4),(4,4)'
          will result in a primary beam covering all the notes and
          secondary beams each group of 4 notes. This beaming
          "directive" can be overridden by using &lt;beam&gt;
          elements. If neither &lt;beam&gt; elements or the
          beam.group attribute is used, then no beaming is rendered.
          Beaming can be 'turned off' by setting beam.group to an
          empty string.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="beam.rests" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether beams should include rests
          shorter than a quarter note duration.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.line" type="data.CLEFLINE">
      <xs:annotation>
        <xs:documentation>contains a default value for the position of
          the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.shape" type="data.CLEFSHAPE">
      <xs:annotation>
        <xs:documentation>encodes a value for the clef
          symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.dis" type="data.OCTAVE.DIS">
      <xs:annotation>
        <xs:documentation>records the amount of octave displacement to
          be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.dis.place" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>records the direction of octave displacement
          to be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dur.default" type="data.DURATIONS">
      <xs:annotation>
        <xs:documentation>used to determine the duration value of the
          first note in a layer if it has no dur
          attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.accid" type="data.ACCIDENTAL.IMPLICIT">
      <xs:annotation>
        <xs:documentation>contains an accidental for the tonic key, if
          one is required, e.g., if key.pname equals 'c' and key.accid
          equals 's', then a tonic of C# is
          indicated.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.mode" type="data.MODE">
      <xs:annotation>
        <xs:documentation>indicates major, minor, or other
          tonality.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.pname" type="data.PITCHNAME">
      <xs:annotation>
        <xs:documentation>holds the pitch name of the tonic key, e.g.
          'c' for the key of C.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig" type="data.KEYSIGNATURE">
      <xs:annotation>
        <xs:documentation>indicates where the key lies in the circle
          of fifths.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig.mixed">
      <xs:annotation>
        <xs:documentation>Mixed key signatures, e.g. those consisting
          of a mixture of flats and sharps (Read, p. 143, ex. 9-39),
          and key signatures with unorthodox placement of the
          accidentals (Read, p. 141) must be indicated by setting the
          key.sig attribute to 'mixed' and providing explicit keysig
          info in the key.sig.mixed attribute or in the
          &lt;keysig&gt; element. It is intended that
          key.sig.mixed contain a series of tokens with each token
          containing pitch name, accidental, and octave, such as 'As4
          Cs5 Ef5' that indicate what key accidentals should be
          rendered and where they should be placed.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                  <xs:pattern value="[A-G](s|f|n|x|ff)[0-9]"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="meter.count" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>captures the number of beats in a measure,
          that is, the top number of the meter
          signature.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="meter.sym" type="data.METERSIGN">
      <xs:annotation>
        <xs:documentation>indicates the use of a meter symbol isntead
          of a numeric meter signature, that is, 'C' for common time
          or 'C' with a slash for cut time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="meter.unit" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>contains the number indicating the beat
          unit, that is, the bottom number of the meter
          signature.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="octave.default" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>functions similarly to dur.default. If the
          oct attribute isn't given on the first note of a measure,
          then the value of octave.default is used.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="source" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>contains a list of one or more pointers
          indicating the sources that contain a staff. Each value
          should correspond to the ID of a &lt;source&gt;
          element given elsewhere in the document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="trans.diat" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>holds the amount of diatonic pitch shift,
          e.g. C to C# = 0, C to Db = 1. Transposition requires both
          trans.diat and trans.semi attributes in order to distinguish
          the difference, for example, between a transposition from C
          to C# and one from C to Db.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="trans.semi" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>contains the amount of pitch shift in
          semitones, C to C# = 1, C to Db = 1. Transposition requires
          both trans.diat and trans.semi attributes in order to
          distinguish the difference, for example, between a
          transposition from C to C# and one from C to
          Db.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.scoredef">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.log.scoredef.base"/>
    <xs:attributeGroup ref="att.log.scoredef.mensural"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.scoredef.mensural">
    <xs:annotation>
      <xs:documentation>Visual domain attributes for scoredef in the
        mensural repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="mensur" type="data.MENSURATIONSIGN">
      <xs:annotation>
        <xs:documentation>the base symbol in the mensuration sign/time
          signature of mensural notation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.dot" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether a dot is to be added to
          the base symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.form">
      <xs:annotation>
        <xs:documentation>indicates whether the base symbol is written
          vertically or horizontally.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="horizontal"/>
          <xs:enumeration value="vertical"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="mensur.loc" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>holds the staff location of the mensuration
          sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.orient">
      <xs:annotation>
        <xs:documentation>describes the rotation or reflection of the
          base symbol.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="reversed"/>
          <xs:enumeration value="90CW"/>
          <xs:enumeration value="90CCW"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="mensur.size" type="data.SIZE">
      <xs:annotation>
        <xs:documentation>describes the relative size of the
          mensuration sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.slash" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>indicates the number lines added to the
          mensuration sign. For example, one slash is added for what
          we now call 'alla breve'.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.scoredef.base">
    <xs:attribute name="bar.place" type="data.BARPLACE">
      <xs:annotation>
        <xs:documentation>describes the placement of
          barlines.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="beam.rend">
      <xs:annotation>
        <xs:documentation>encodes whether a beam is feathered and in
          which direction.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="acc"/>
          <xs:enumeration value="rit"/>
          <xs:enumeration value="norm"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="beam.slope" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>captures default beam
          slope.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.color" type="data.COLOR">
      <xs:annotation>
        <xs:documentation>describes the color of the
          clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether the clef is to be
          displayed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dynam.dist" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the default distance from the staff
          of dynamic marks.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ending.rend">
      <xs:annotation>
        <xs:documentation>describes where ending marks should be
          displayed.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="top"/>
          <xs:enumeration value="barred"/>
          <xs:enumeration value="grouped"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="grid.show" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether to display guitar chord
          grids.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="harm.dist" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the default distance from the staff
          of harmonic indications, such as guitar chord grids or
          functional labels.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig.show" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether the key signature should
          be displayed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig.showchange" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether cautionary accidentals
          should be displayed at a key change.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.align" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the alignment of lyric syllables
          associated with a note or chord.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.fam" type="data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>sets the font family default value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.name" type="data.FONTNAME">
      <xs:annotation>
        <xs:documentation>sets the font name default value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.size" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>sets the default font size value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.style" type="data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>sets the default font style value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.weight" type="data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>sets the default font weight value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="meter.rend">
      <xs:annotation>
        <xs:documentation>contains indication of how the meter
          signature should be rendered.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="denomsym"/>
          <xs:enumeration value="norm"/>
          <xs:enumeration value="invis"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="meter.sig.showchange" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether a new meter signature
          should be displayed when the meter signature
          changes.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mnum.visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether measure numbers should be
          displayed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="multi.number" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether programmatically
          calculated counts of multiple measures of rest (mrest) and
          whole measure repeats (mrpt) in parts should be
          rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="music.name" type="data.MUSICFONT">
      <xs:annotation>
        <xs:documentation>sets the default music font
          name.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="music.size" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>sets the default music font
          size.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ontheline" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines the placement of notes on a
          1-line staff. A value of 'yes' places all notes on the line,
          while a value of 'no' places stems-up notes above the line
          and stems-down notes below the line.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="optimize" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>display only those staves with
          notes?</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.height" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the physical height of the
          rendered output page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.width" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the physwical width of the
          rendered output page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.units" type="data.PGUNITS">
      <xs:annotation>
        <xs:documentation>contains the real-world measurement units
          (inches, centimeters, millimeters) used to describe the
          rendered page height and width.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.topmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the
          top of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.botmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the
          bottom of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.leftmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the
          left side of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.rightmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at the
          right side of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.panels">
      <xs:annotation>
        <xs:documentation>indicates the number of logical pages to be
          rendered on a single physical page.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="1"/>
          <xs:enumeration value="2"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="page.scale" type="data.PGSCALE">
      <xs:annotation>
        <xs:documentation>indicates how the page should be scaled when
          rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pedal.rend">
      <xs:annotation>
        <xs:documentation>determines whether piano pedal marks should
          be drawn as lines or as terms (e.g. 'Ped.' for pedal down
          and '*' for pedal up).</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="line"/>
          <xs:enumeration value="term"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="reh.enclose">
      <xs:annotation>
        <xs:documentation>describes the enclosing shape for rehearsal
          marks.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="box"/>
          <xs:enumeration value="circle"/>
          <xs:enumeration value="none"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="slur.rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>describes the default rendering style for
          slurs.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.packexp" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes a note's spacing relative to its
          time value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.packfact" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the note spacing of
          output.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.staff" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>sets the minimum amount of space between
          staves in the same system.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.system">
      <xs:annotation>
        <xs:documentation>contains a space-separated pair of numbers
          describing the minimum and maximum amount of space between
          systems.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="system.leftmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the amount of whitespace at the
          left system margin relative to
          page.leftmar</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="system.rightmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the amount of whitespace at the
          right system margin relative to
          page.rightmar</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="system.topmar" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the distance from page's top edge
          to the first system; used for first page
          only</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.dist" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>determines how far from the staff to render
          text elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.fam" type="data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>sets the default font family value for text
          other than lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.name" type="data.FONTNAME">
      <xs:annotation>
        <xs:documentation>sets the default font name value for text
          other than lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.size" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>sets the default font size value for text
          other than lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.style" type="data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>sets the default font style value for text
          other than lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.weight" type="data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>sets the default font weight value for text
          other than lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tie.rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>describes the default rendering style for
          ties.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.scoredef">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.vis.scoredef.base"/>
    <xs:attributeGroup ref="att.vis.scoredef.mensural"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for section                                     * -->
  <xs:attributeGroup name="att.anl.section">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.section">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="expan" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>contains a list of ids of child
          &lt;section&gt;, &lt;ending&gt; or
          &lt;app&gt; elements. This 'expansion list'
          indicates how the section should be expanded into its
          'through-composed' form. See Read, p. 226 (ex. 12-10),
          228-229 (ex. 12-16).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for staff                                       * -->
  <xs:attributeGroup name="att.anl.staff">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.staff">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates if a staff should be displayed
          when the score is rendered. A staff with this attribute set
          to 'false' should also not be included in a sounded
          rendition.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for staffdef                                    * -->
  <xs:attributeGroup name="att.ges.staffdef.tablature">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes for staffdef in
        tablature</xs:documentation>
    </xs:annotation>
    <xs:attribute name="tab.strings">
      <xs:annotation>
        <xs:documentation>provides a written pitch and octave for each
          open string, e.g. "e3 a3 d4 g4 b4 e5" for standard guitar
          tuning. It should contain the number of tokens indicated by
          the lines attribute. Chromatic alteration may be indicated
          with '-' (flat) and '#' (sharp). Double sharps and flats are
          not permitted.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.staffdef.base">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes for staffdef in the
        CMN repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="grace" type="data.GRACE">
      <xs:annotation>
        <xs:documentation>Setting the grace attribute here provides
          the possibility of changing all the grace attribute values
          in a staff without having to individually change each
          event's grace attribute value. Each "graced" element may
          still require a value in its grace.time
          attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="midi.div" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>sets the number of MIDI ticks per quarter
          note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.staffdef">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes for staffdef in all
        repertoires</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.ges.staffdef.base"/>
    <xs:attributeGroup ref="att.ges.staffdef.tablature"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.staffdef.mensural">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for staffdef in the
        mensural repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="mensur.sign" type="data.MENSURATIONSIGN">
      <xs:annotation>
        <xs:documentation>the base symbol in the mensuration sign/time
          signature of mensural notation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.dot" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines if a dot is to be added to the
          base symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.slash" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>indicates the number lines added to the
          mensuration sign. For example, one slash is added for what
          we now call 'alla breve'.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="modusmaior">
      <xs:annotation>
        <xs:documentation>describes the maxima-long
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="modusminor">
      <xs:annotation>
        <xs:documentation>describes the long-breve
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with numbase, describes duration as a
          ratio. num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="numbase" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with num, describes duration as a
          ratio. num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="prolatio">
      <xs:annotation>
        <xs:documentation>describes the semibreve-minim
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="proport.num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>Together, proport.num and proport.numbase
          specify a proportional change as a ratio, e.g., 1:3.
          Proport.um is for the first value in the
          ratio.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="proport.numbase" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>Together, proport.num and proport.numbase
          specify a proportional change as a ratio, e.g., 1:3.
          Proport.numbase is for the second value in the
          ratio.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tempus">
      <xs:annotation>
        <xs:documentation>describes the breve-semibreve
          relationship.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.staffdef.base">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for staffdef in the
        CMN repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="beam.group">
      <xs:annotation>
        <xs:documentation>provides an example of how beaming
          (including secondary beams) is to be performed. It can be
          used to set a default beaming pattern to be used when no
          beaming is indicated at the event level. beam.group must
          contain a comma-separated list of time values that add up to
          a measure, e.g., in 4/4 time '4,4,4,4' indicates each
          quarter note worth of shorter notes would be beamed
          together. Parentheses can be used to indicate sub-groupings
          of secondary beams. For example, '(4.,4.,4.)' in 9/8 meter
          indicates one outer beam per measure with secondary beams
          broken at each dotted quarter duration, while a measure of
          16th notes in 4/4 with beam.group equal to '(4,4),(4,4)'
          will result in a primary beam covering all the notes and
          secondary beams each group of 4 notes. This beaming
          "directive" can be overridden by using &lt;beam&gt;
          elements. If neither &lt;beam&gt; elements or the
          beam.group attribute is used, then no beaming is rendered.
          Beaming can be 'turned off' by setting beam.group to an
          empty string.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="beam.rests" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether rests are allowed under a
          beam.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.line" type="data.CLEFLINE">
      <xs:annotation>
        <xs:documentation>contains a default value for the position of
          the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.shape" type="data.CLEFSHAPE">
      <xs:annotation>
        <xs:documentation>encodes a value for the clef
          symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.dis" type="data.OCTAVE.DIS">
      <xs:annotation>
        <xs:documentation>records the amount of octave displacement to
          be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.dis.place" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>records the direction of octave displacement
          to be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dur.default" type="data.DURATIONS">
      <xs:annotation>
        <xs:documentation>contains a default duration in those
          situations when the first note, rest, chord, etc. in a
          measure does not have a duration
          specified.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.accid" type="data.ACCIDENTAL.IMPLICIT">
      <xs:annotation>
        <xs:documentation>contains an accidental for the tonic key, if
          one is required, e.g., if key.pname equals 'c' and key.accid
          equals 's', then a tonic of C# is
          indicated.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.mode" type="data.MODE">
      <xs:annotation>
        <xs:documentation>indicates major or minor
          tonality.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.pname" type="data.PITCHNAME">
      <xs:annotation>
        <xs:documentation>holds the pitch name of the tonic key, e.g.
          'c' for the key of C.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig" type="data.KEYSIGNATURE">
      <xs:annotation>
        <xs:documentation>indicates where the key is in the circle of
          fifths.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig.mixed">
      <xs:annotation>
        <xs:documentation>Mixed key signatures, e.g. those consisting
          of a mixture of flats and sharps (Read, p. 143, ex. 9-39),
          and key signatures with unorthodox placement of the
          accidentals (Read, p. 141) must be indicated by setting the
          key.sig attribute to 'mixed' and providing explicit keysig
          info in the key.sig.mixed attribute or in the
          &lt;keysig&gt; element. It is intended that
          key.sig.mixed contain a series of tokens with each token
          containing pitch name, accidental, and octave, such as 'As4
          Cs5 Ef5' that indicate what key accidentals should be
          rendered and where they should be placed.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:restriction base="xs:NMTOKEN">
                  <xs:pattern value="[A-G](s|f|n|x|ff)[0-9]"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="meter.count" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>captures the number of beats in a measure,
          that is, the top number of the meter
          signature.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="meter.sym" type="data.METERSIGN">
      <xs:annotation>
        <xs:documentation>indicates the use of a meter symbol isntead
          of a numeric meter signature, that is, 'C' for common time
          or 'C' with a slash for cut time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="meter.unit" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>contains the number indicating the beat
          unit, that is, the bottom number of the meter
          signature.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="octave.default" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>functions similarly to dur.default. If the
          oct attribute isn't given on the first note of a measure,
          then the value of octave.default is used.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="trans.diat" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>holds the amount of diatonic pitch shift,
          e.g. C to C# = 0, C to Db = 1. Transposition requires both
          trans.diat and trans.semi attributes in order to distinguish
          the difference, for example, between a transposition from C
          to C# and one from C to Db.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="trans.semi" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>contains the amount of pitch shift in
          semitones, C to C# = 1, C to Db = 1. Transposition requires
          both trans.diat and trans.semi attributes in order to
          distinguish the difference, for example, between a
          transposition from C to C# and one from C to
          Db.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.staffdef">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for staffdef in all
        repertoires</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.log.staffdef.base"/>
    <xs:attributeGroup ref="att.log.staffdef.mensural"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.staffdef.mensural">
    <xs:annotation>
      <xs:documentation>Visual domain attributes for the mensural
        repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="mensur.form">
      <xs:annotation>
        <xs:documentation>indicates whether the base symbol is written
          vertically or horizontally.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="horizontal"/>
          <xs:enumeration value="vertical"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="mensur.loc" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>holds the staff location of the mensuration
          sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mensur.orient">
      <xs:annotation>
        <xs:documentation>describes the rotation or reflection of the
          base symbol.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="reversed"/>
          <xs:enumeration value="90CW"/>
          <xs:enumeration value="90CCW"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="mensur.size" type="data.SIZE">
      <xs:annotation>
        <xs:documentation>describes the relative size of the
          mensuration sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.staffdef.base">
    <xs:annotation>
      <xs:documentation>Visual domain attributes for staffdef in the
        CMN repertoire</xs:documentation>
    </xs:annotation>
    <xs:attribute name="beam.rend">
      <xs:annotation>
        <xs:documentation>encodes whether a beam is feathered and in
          which direction.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="acc"/>
          <xs:enumeration value="rit"/>
          <xs:enumeration value="norm"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="beam.slope" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>captures default beam
          slope.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.color" type="data.COLOR">
      <xs:annotation>
        <xs:documentation>describes the color of the
          clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="clef.visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether the clef is to be
          displayed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dynam.dist" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the default distance from the staff
          of dynamic marks.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="grid.show" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether to display guitar chord
          grids.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="harm.dist" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the default distance from the staff
          of harmonic indications, such as guitar chord grids or
          functional labels.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig.show" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether the key signature should
          be displayed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key.sig.showchange" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether cautionary accidentals
          should be displayed at a key change.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="label.full">
      <xs:annotation>
        <xs:documentation>provides a label for a group of staves on
          the first page; label.abbr is used
          thereafter.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="label.abbr">
      <xs:annotation>
        <xs:documentation>provides a label for a group of staves on
          pages after the first page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="layerscheme" type="data.LAYERSCHEME">
      <xs:annotation>
        <xs:documentation>indicates the number of layers and their
          stem directions.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lines" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>indicates the number of
          stafflines.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lines.color" type="xs:NMTOKENS">
      <xs:annotation>
        <xs:documentation>encodes the colors of the staff lines. The
          value is structured; that is, it should have the same number
          of space-separated RGB values as the number of lines
          indicated by the lines attribute. A line can be made
          invisible by assigning it the same RGB value as the
          background, usually white.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lines.visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>records whether all stafflines are
          visible.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.align" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>describes the alignment of lyric syllables
          associated with a note or chord.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.fam" type="data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>sets the font family default value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.name" type="data.FONTNAME">
      <xs:annotation>
        <xs:documentation>sets the font name default value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.size" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>sets the default font size value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.style" type="data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>sets the default font style value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lyric.weight" type="data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>sets the default font weight value for
          lyrics.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="meter.rend">
      <xs:annotation>
        <xs:documentation>contains an indication of how the meter
          signature should be rendered.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="denomsym"/>
          <xs:enumeration value="norm"/>
          <xs:enumeration value="invis"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="meter.sig.showchange" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines whether a new meter signature
          should be displayed when the meter signature
          changes.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="multi.number" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether programmatically
          calculated counts of multiple measures of rest (mrest) and
          whole measure repeats (mrpt) in parts should be
          rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ontheline" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>determines the placement of notes on a
          1-line staff. A value of 'yes' places all notes on the line,
          while a value of 'no' places stems-up notes above the line
          and stems-down notes below the line.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pedal.rend">
      <xs:annotation>
        <xs:documentation>determines whether piano pedal marks should
          be drawn as lines or as terms (e.g. 'Ped.' for pedal down
          and '*' for pedal up).</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="line"/>
          <xs:enumeration value="term"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="reh.enclose">
      <xs:annotation>
        <xs:documentation>describes the enclosing shape for rehearsal
          marks.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="box"/>
          <xs:enumeration value="circle"/>
          <xs:enumeration value="none"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="scale" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>indicates how the staff should be scaled
          when rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slur.rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>describes the default rendering style for
          slurs.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the absolute distance (as opposed to
          the relative distances recorded in &lt;scoredef&gt;
          elements) between this staff and the preceding one in the
          same system. This value is meaningless for the first staff
          in a system since the spacing.system attribute indicates the
          spacing between systems.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.dist" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>determines how far from the staff to render
          text elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.fam" type="data.FONTFAMILY">
      <xs:annotation>
        <xs:documentation>provides a default value for the font family
          name for text (other than lyrics) associated with this
          staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.name" type="data.FONTNAME">
      <xs:annotation>
        <xs:documentation>provides a default value for the font name
          for text (other than lyrics) associated with this
          staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.size" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>provides a default value for the font size
          for text (other than lyrics) associated with this
          staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.style" type="data.FONTSTYLE">
      <xs:annotation>
        <xs:documentation>provides a default value for the font style
          for text (other than lyrics) associated with this
          staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text.weight" type="data.FONTWEIGHT">
      <xs:annotation>
        <xs:documentation>provides a default value for the font weight
          for text (other than lyrics) associated with this
          staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tie.rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>provides a default value for the appearance
          of ties associated with this staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates if a staff should be displayed
          when the score is rendered. A staff with this attribute set
          to 'false' should also not be included in a sounded
          rendition.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.staffdef">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.vis.staffdef.base"/>
    <xs:attributeGroup ref="att.vis.staffdef.mensural"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for staffgrp                                    * -->
  <xs:attributeGroup name="att.ges.staffgrp">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="midi.div" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>sets the default number of MIDI ticks per
          quarter note for all staves in the group.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREFS">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.staffgrp">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="barthru" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether barlines go across the
          space between staves (yes) or are only drawn across the
          stafflines of each staff (no)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="label.abbr">
      <xs:annotation>
        <xs:documentation>provides a label for a group of staves on
          pages after the first page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="label.full">
      <xs:annotation>
        <xs:documentation>provides a label for a group of staves on
          the first page; label.abbr is used
          thereafter.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="symbol">
      <xs:annotation>
        <xs:documentation>indicates the left-hand connecting symbol
          for the staff group.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="brace"/>
          <xs:enumeration value="bracket"/>
          <xs:enumeration value="line"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates if a staff group should be
          displayed when the notation is rendered. A staff group with
          this attribute set to 'false' should also not be included in
          a sounded rendition.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for uneume                                      * -->
  <xs:attributeGroup name="att.anl.uneume">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
    <xs:attribute name="accid.cautionary"
      type="data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>indicates a cautionary accidental
          accompanying the neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="accid.editorial"
      type="data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>indicates an editorial accidental
          accompanying the neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="hfunc">
      <xs:annotation>
        <xs:documentation>contains an indication of the harmonic
          function of this neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="inth">
      <xs:annotation>
        <xs:documentation>encodes the harmonic interval from a pitch
          occuring at the same time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="intm">
      <xs:annotation>
        <xs:documentation>encodes the melodic interval from the
          previous neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mfunc">
      <xs:annotation>
        <xs:documentation>contains an indication of the melodic
          function of this element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pclass" type="data.PITCHCLASS">
      <xs:annotation>
        <xs:documentation>holds pitch class
          information.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="psolfa">
      <xs:annotation>
        <xs:documentation>contains solfa designation, e.g., do, re,
          mi, etc., in either fixed or movable Do
          systems.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.uneume">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="form" type="data.UNEUMEFORM">
      <xs:annotation>
        <xs:documentation>records the function of the
          neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="name" type="data.UNEUMENAME">
      <xs:annotation>
        <xs:documentation>records the name of the
          neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="syl">
      <xs:annotation>
        <xs:documentation>holds an associated sung text
          syllable.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.uneume">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset.ho"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="size" type="data.SIZE">
      <xs:annotation>
        <xs:documentation>encodes the relative size of the
          neume.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="visible" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates if a neume should be displayed
          when the notation is rendered. A neume with this attribute
          set to 'false' should also not be included in a sounded
          rendition.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for accid                                       * -->
  <xs:attributeGroup name="att.anl.accid">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.accid">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="func">
      <xs:annotation>
        <xs:documentation>If an accidental has an editorial or
          cautionary function, its function may be captured in the
          func attribute.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="caution"/>
          <xs:enumeration value="edit"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="value" use="required"
      type="data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>records a written
          accidental.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.accid">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset.ho"/>
    <xs:attributeGroup ref="att.visualoffset.vo"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="enclose" type="data.ENCLOSURE">
      <xs:annotation>
        <xs:documentation>records the characters often used to mark
          the accidental as having an cautionary or editorial
          function. For an example of cautionary accidentals enclosed
          in parentheses, see Read, p. 131, ex.
          9-14.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="place">
      <xs:annotation>
        <xs:documentation>captures the placement of the accidental
          relative to the staff.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="above"/>
          <xs:enumeration value="below"/>
          <xs:enumeration value="staff"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for annot                                       * -->
  <xs:attributeGroup name="att.anl.annot">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.annot">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records the durational scope of the
          annotation when it differs from the written values of the
          participants. Its value may be expressed in one of several
          forms, such as measures[s]+ beat[s].beatpart, ppq (MIDI
          clicks), Humdrum **recip values, or MusicXML 'divisions',
          etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.annot">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for annot. Values
        for the type attribute can be taken from any convenient
        typology of annotation suitable to the work in hand; e.g.
        annotation, gloss, citation, digression, preliminary,
        temporary, etc.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="dur" type="data.MEASUREBEAT">
      <xs:annotation>
        <xs:documentation>records the durational scope of the
          annotation. Please note that the dur attribute is not a true
          duration, but rather a time stamp for the end point of the
          directive.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="layer" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the layer to which an annotation
          applies.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="staff" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the staff to which an annotation
          applies.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp" type="data.BEAT">
      <xs:annotation>
        <xs:documentation>encodes the onset time in terms of musical
          time, i.e., beats[.fractional beat part]</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.ges" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>records the onset time in pulses per quarter
          note (ppq, MusicXML divisions, or MIDI clicks) since the
          start of the measure.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.real" type="data.ISOTIME">
      <xs:annotation>
        <xs:documentation>records the onset time as a real-time
          offset, i.e., in HH:MM:SS.ss format, since the beginning of
          the file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for arpeg                                       * -->
  <xs:attributeGroup name="att.anl.arpeg">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.arpeg">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attribute name="dir">
      <xs:annotation>
        <xs:documentation>describes the direction in which the
          arpeggio is to be performed; "nonarp" indicates a
          non-arpeggiated chord, usually rendered with a preceding
          bracket instead of a wavy line.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="up"/>
          <xs:enumeration value="down"/>
          <xs:enumeration value="nonarp"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.arpeg">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="arrow" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>may be used when an arrowhead is to be drawn
          as part of the arpeggiation symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for artic                                       * -->
  <xs:attributeGroup name="att.anl.artic">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.artic">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="value" use="required" type="data.ARTICULATION">
      <xs:annotation>
        <xs:documentation>records the name of the articulation symbol
          drawn from the list given in
          data.ARTICULATIONS.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.artic">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="enclose" type="data.ENCLOSURE">
      <xs:annotation>
        <xs:documentation>encodes the characters used to enclose the
          accidental when it has a cautionary or editorial
          function.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for beam                                        * -->
  <xs:attributeGroup name="att.anl.beam">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.beam">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="with" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>In the case of cross-staff beams, the with
          attribute is used to indicate which staff the beam is
          connected to; that is, the staff above or the staff
          below.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.beam">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="breaksec" type="xs:NMTOKENS">
      <xs:annotation>
        <xs:documentation>encodes the explicit breaking of secondary
          beams. The value should contain a space-separated list of
          rhythmic durations that form a single beat. For example,'8
          8' indicates a division of the beam into two 8th-note
          groups.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rend">
      <xs:annotation>
        <xs:documentation>captures whether a beam is feathered and in
          which direction: 'acc' indicates that the secondary beams
          get progressively closer together, coming together at the
          end of the beam, while 'rit' means that the secondary beams
          start at the same point and get progressively more
          distant.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="acc"/>
          <xs:enumeration value="rit"/>
          <xs:enumeration value="norm"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="slope" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the slope of the
          beam.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for beamspan                                    * -->
  <xs:attributeGroup name="att.anl.beamspan">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.beamspan">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.beamspan">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="dur" type="data.MEASUREBEAT">
      <xs:annotation>
        <xs:documentation>encodes the logical and visual duration of
          the beam. Please note that the dur attribute is not a true
          duration, but rather a time stamp for the end point of the
          trill.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="with" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>In the case of cross-staff beams, the with
          attribute is used to indicate which staff the beam is
          connected to; that is, the staff above or the staff
          below.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.beamspan">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="breaksec" type="xs:NMTOKENS">
      <xs:annotation>
        <xs:documentation>encodes the explicit breaking of secondary
          beams. The value should contain a space-separated list of
          rhythmic durations that form a single beat. For example,'8
          8' indicates a division of the beam into two 8th-note
          groups.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rend">
      <xs:annotation>
        <xs:documentation>captures whether a beam is feathered and in
          which direction: 'acc' indicates that the secondary beams
          get progressively closer together, coming together at the
          end of the beam, while 'rit' means that the secondary beams
          start at the same point and get progressively more
          distant.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="acc"/>
          <xs:enumeration value="rit"/>
          <xs:enumeration value="norm"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="slope" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the slope of the
          beam.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for beatrpt                                     * -->
  <xs:attributeGroup name="att.anl.beatrpt">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.beatrpt">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.beatrpt">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attribute name="expand" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether to render the repeated
          beat symbol or the source material.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rend" use="required" type="data.BEATRPT.REND">
      <xs:annotation>
        <xs:documentation>indicates the number of slashes required to
          render the appropriate beat repeat symbol when the beat is
          divided into even notes: 4ths or 8ths=1, 16ths=2, 32nds=3,
          64ths=4, 128ths=5. When the beat is comprised of mixed
          duration values, the symbol is always rendered as 2 slashes
          and 2 dots. See Read, pg. 223-224.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for bend                                        * -->
  <xs:attributeGroup name="att.anl.bend">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.bend">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="amount" type="data.BEND.AMOUNT">
      <xs:annotation>
        <xs:documentation>records the amount of detuning. The decimal
          values should be rendered as a fraction (or an integer plus
          a fraction) along with the bend symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.bend">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.bend">
    <xs:annotation>
      <xs:documentation>Visual domain attributes. If the bulge or
        bezier attributes are present, the bend should be rendered as
        a curve. Otherwise, it should be rendered using lines. The ho
        and vo attributes describe the visual offset of the entire
        rendered bend. The endho, endvo and startho, startvo attribute
        pairs may be used to encode start and end points relative to
        their programmatic placement. For exact placement of the
        endpoints of the bend, use the x and y
        attributes.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.visualoffset2"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attributeGroup ref="att.xy2"/>
    <xs:attribute name="bezier" type="xs:NMTOKENS">
      <xs:annotation>
        <xs:documentation>records the placement of Bezier control
          points as a series of space-separated xy coordinates, eg. 19
          45 -32 118.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="bulge" type="xs:NMTOKENS">
      <xs:annotation>
        <xs:documentation>describes the curve as a set of distance
          values above or below an imaginary line connecting the
          endpoints of the curve. The bulge attribute must contain one
          or more values of type NUMBER.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rend" type="data.CURVERENDITION">
      <xs:annotation>
        <xs:documentation>records the appearance of the curve or
          line.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for breath                                      * -->
  <xs:attributeGroup name="att.anl.breath">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.breath">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.breath">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for btrem                                       * -->
  <xs:attributeGroup name="att.anl.btrem">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.btrem">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="form">
      <xs:annotation>
        <xs:documentation>describes the bowed tremolo as measured or
          unmeasured.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="meas"/>
          <xs:enumeration value="unmeas"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>used to store a number to be rendered along
          with the note. See Read, p. 394, ex. 23-21, line
          5.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slash" type="data.SLASH">
      <xs:annotation>
        <xs:documentation>indicates the number of slashes to be drawn.
          The number of slashes indicates the exact note values in an
          aural rendition of measured tremolandi, i.e., 4ths or
          8ths=1, 16ths=2, 32nds=3, 64ths=4, 128ths=5, 256ths=6. See
          Read, p. 393-394. The stem modifier (slash or z) must also
          be explicity set on the child &lt;note&gt; or
          &lt;chord&gt; element for a complete visual
          representation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.btrem">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="num.place" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>records where the number accompanying the
          tremolo note or chord should be rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for chord                                       * -->
  <xs:attributeGroup name="att.anl.chord">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
    <xs:attribute name="hfunc">
      <xs:annotation>
        <xs:documentation>describes the chord's harmonic function in
          any convenient typology.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mfunc">
      <xs:annotation>
        <xs:documentation>describes the chord's melodic function in
          any convenient typology.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.chord">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="artic.ges" type="data.ARTICULATIONS">
      <xs:annotation>
        <xs:documentation>records performed articulation that differs
          from the written value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="detune"/>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc. </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="grace" type="data.GRACE">
      <xs:annotation>
        <xs:documentation>marks this chord as a grace and records from
          which note/chord it should "steal" time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="grace.time" type="data.PERCENT">
      <xs:annotation>
        <xs:documentation>records the amount of time to be "stolen"
          from a non-grace note/chord.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.chord">
    <xs:annotation>
      <xs:documentation>Logical domain attributes for chord. The
        artic, dots, and dur attributes encode the written
        articulations, augmentation dots, and duration values. The
        beam, fermata, lv, slur, syl, tie, and tuplet attributes may
        be used to indicate the attachment of these things to this
        chord. If visual information about these things needs to be
        recorded, then either the elements corresponding to these
        attributes or the attributes available in the att.vis.chord
        class should be employed.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="artic" type="data.ARTICULATIONS">
      <xs:annotation>
        <xs:documentation>encodes the written articulation(s).
          Articulations are normally encoded in order from the
          notehead outward; that is, away from the stem. See
          additional notes at att.vis.note. Only articulations should
          be encoded in the artic attribute; fingerings should be
          encoded using the &lt;dir&gt;
          element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="beam" type="data.BEAMS">
      <xs:annotation>
        <xs:documentation>indicates that this event is "under a
          beam".</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dots" type="data.AUGMENTDOT">
      <xs:annotation>
        <xs:documentation>encodes the number of written dots of
          augmentation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dur" type="data.DURATION">
      <xs:annotation>
        <xs:documentation>encodes logical; that is, written,
          duration.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fermata" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>indicates the attachment of a fermata to
          this element. If visual information about the fermata needs
          to be recorded, then a &lt;fermata&gt; element
          should be employed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="lv" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates the attachment of an l.v. (laissez
          vibrer) sign to this element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slur" type="data.SLURS">
      <xs:annotation>
        <xs:documentation>indicates that this element participates in
          a slur.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="syl">
      <xs:annotation>
        <xs:documentation>holds an associated sung text
          syllable.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tie" type="data.TIE">
      <xs:annotation>
        <xs:documentation>indicates that this element participates in
          a tie.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tuplet" type="data.TUPLET">
      <xs:annotation>
        <xs:documentation>indicates that this element participates in
          a tuplet.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.chord">
    <xs:annotation>
      <xs:documentation>Visual domain attributes for chord. The slur,
        slur.dir, slur.rend, tie, tie.dir, and tie.rend attributes
        here are syntactic sugar for these attributes on each of the
        chord's individual notes. The values here apply to all the
        notes in the chord. If some notes are slurred or tied while
        others aren't, then the individual note attributes must be
        used.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset.ho"/>
    <xs:attributeGroup ref="att.visualoffset.to"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="beam.with" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>In the case of cross-staff beams, the with
          attribute is used to indicate which staff the beam is
          connected to; that is, the staff above or the staff
          below.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="coloration">
      <xs:annotation>
        <xs:documentation>indicates inverse coloration; that is, the
          notehead is void when it would otherwise be filled and vice
          versa. Do not confuse this with visual
          color.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="inverse"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="dot.ho" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records the horizontal offset of the
          augmentation dot from its "normal", programmatically
          determined position.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dot.vo" type="data.VISUALOFFSET">
      <xs:annotation>
        <xs:documentation>records the vertical offset of the
          augmentation dot from its "normal", programmatically
          determined position.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dot.x" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the absolute horizontal position of
          the augmentation dot.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dot.y" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the absolute vertical position of
          the augmentation dot.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="headshape" type="data.CLUSTER">
      <xs:annotation>
        <xs:documentation>indicates an alternative notehead should be
          displayed instead of individual noteheads. See Read, p.
          320-321, re: tone clusters.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="size" type="data.SIZE">
      <xs:annotation>
        <xs:documentation>describes the relative size of the
          chord.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slur.dir" type="data.SLURDIRECTION">
      <xs:annotation>
        <xs:documentation>indicates the direction of a
          slur.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="slur.rend">
      <xs:annotation>
        <xs:documentation>describes the rendering style for
          slurs.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="dotted"/>
          <xs:enumeration value="dashed"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="stem.dir" type="data.STEMDIRECTION">
      <xs:annotation>
        <xs:documentation>describes the direction of a
          stem.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stem.len" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>encodes the stem length.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stem.mod" type="data.STEMMODIFIER">
      <xs:annotation>
        <xs:documentation>encodes any stem "modifiers"; that is,
          symbols rendered on the stem, such as tremolo or
          Sprechstimme indicators.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stem.pos" type="data.STEMPOSITION">
      <xs:annotation>
        <xs:documentation>records the position of the stem in relation
          to the notehead(s).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stem.with" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>contains an indication of which staff a note
          or chord that logically belongs to the current staff should
          be visually placed on.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stem.x" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the x coordinate of the stem's
          attachment point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="stem.y" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>records the y coordinate of the stem's
          attachment point.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tie.dir" type="data.TIEDIRECTION">
      <xs:annotation>
        <xs:documentation>determines the direction of a
          tie.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tie.rend">
      <xs:annotation>
        <xs:documentation>describes the rendering style of a
          tie.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="dotted"/>
          <xs:enumeration value="dashed"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for clefchange                                  * -->
  <xs:attributeGroup name="att.anl.clefchange">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.clefchange.base">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="line" type="data.CLEFLINE">
      <xs:annotation>
        <xs:documentation>records the position of the clef. The value
          must be in the range between 1 and the number of lines on
          the staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="octave.default" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>sets the value of the oct attribute of the
          event following the clefchange, if the event doesn't already
          have an oct attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="shape" type="data.CLEFSHAPE">
      <xs:annotation>
        <xs:documentation>indicates the shape of the clef
          symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dis" type="data.OCTAVE.DIS">
      <xs:annotation>
        <xs:documentation>records the amount of octave displacement to
          be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dis.place" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>records the direction of octave displacement
          to be applied to the clef.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.clefchange">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.log.clefchange.base"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.clefchange">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for dir                                         * -->
  <xs:attributeGroup name="att.anl.dir">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.dir">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.dir">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="dur" type="data.MEASUREBEAT">
      <xs:annotation>
        <xs:documentation>encodes the logical and visual durations of
          the directive. If cases arise where the logical and visual
          durations differ, it may be necessary to add a dur.vis
          attribute. Please note that the dur attribute is not a true
          duration, but rather a time stamp for the end point of the
          directive.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.dir">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.visualoffset2.ho"/>
    <xs:attributeGroup ref="att.visualoffset2.to"/>
    <xs:attributeGroup ref="att.xy"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for dynam                                       * -->
  <xs:attributeGroup name="att.anl.dynam">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.dynam">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc. </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="value" type="data.MIDIVALUE">
      <xs:annotation>
        <xs:documentation>captures a numeric value associated with the
          graphical dynamic sign.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.dynam">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="dur" type="data.MEASUREBEAT">
      <xs:annotation>
        <xs:documentation>encodes the logical and visual durations of
          the dynamic symbol or text. If cases arise where the logical
          and visual durations differ, it may be necessary to add a
          dur.vis attribute. Please note that the dur attribute is not
          a true duration, but rather a time stamp for the end point
          of the dynamic mark.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.dynam">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.visualoffset2.ho"/>
    <xs:attributeGroup ref="att.visualoffset2.to"/>
    <xs:attributeGroup ref="att.xy"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for fermata                                     * -->
  <xs:attributeGroup name="att.anl.fermata">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.fermata">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc. </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.fermata">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.fermata">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="form">
      <xs:annotation>
        <xs:documentation>describes the visual appearance of the
          fermata.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="inv"/>
          <xs:enumeration value="norm"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for ftrem                                       * -->
  <xs:attributeGroup name="att.anl.ftrem">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.ftrem">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="form">
      <xs:annotation>
        <xs:documentation>describes whether the tremolo is measured or
          unmeasured.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="meas"/>
          <xs:enumeration value="unmeas"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.ftrem">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.ftrem">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="slash" use="required" type="data.SLASH">
      <xs:annotation>
        <xs:documentation>indicates the number of slashes required 
          4ths or 8ths=1, 16ths=2, 32nds=3, 64ths=4, 128ths=5,
          256ths=6.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for gliss                                       * -->
  <xs:attributeGroup name="att.anl.gliss">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.gliss">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.gliss">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.visualoffset2"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attributeGroup ref="att.xy2"/>
    <xs:attribute name="rend" type="data.LINERENDITION">
      <xs:annotation>
        <xs:documentation>captures the appearance of the glissando
          mark.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="text">
      <xs:annotation>
        <xs:documentation>records any text, such as 'gliss', that
          accompanies the glissando mark.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for hairpin                                     * -->
  <xs:attributeGroup name="att.anl.hairpin">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.hairpin">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.hairpin">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="dur" type="data.MEASUREBEAT">
      <xs:annotation>
        <xs:documentation>encodes the logical and visual duration of
          the hairpin. If the logical and visual durations differ, it
          may be necessary to add a dur.vis attribute. Please note
          that the dur attribute is not a true duration, but rather a
          time stamp for the end point of the
          hairpin.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="form" use="required">
      <xs:annotation>
        <xs:documentation>captures the function of the
          hairpin.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="cres"/>
          <xs:enumeration value="dim"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.hairpin">
    <xs:annotation>
      <xs:documentation>Visual domain attributes. The startho and
        startvo attributes record the horizontal and vertical offsets
        of the left end, endho and endvo record the horizontal and
        vertical offsets of the right end, and the width attribute
        records the width of the opening on the open end in one-half
        staff inter-line units. The x1 and y1 attributes give the
        absolute coordinates of the left side of the hairpin while x2
        and y2 record the coordinates of the right
        side.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.visualoffset2"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attributeGroup ref="att.xy2"/>
    <xs:attribute name="width" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>specifies the distance, in one-half
          interline units, between the points of the open end of the
          mark.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for halfmrpt                                    * -->
  <xs:attributeGroup name="att.anl.halfmrpt">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.halfmrpt">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="dur" type="data.DURATION">
      <xs:annotation>
        <xs:documentation>states the logical duration of the repeat
          symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.halfmrpt">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attribute name="expand" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether to render the source
          material.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for harm                                        * -->
  <xs:attributeGroup name="att.anl.harm">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.harm">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.harm">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="chordref" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>contains a reference to a predefined
          &lt;chorddef&gt; element.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dur" type="data.MEASUREBEAT">
      <xs:annotation>
        <xs:documentation>encodes the logical and visual duration of
          the harmony. If the logical and visual durations differ, it
          may be necessary to add a dur.vis attribute. Please note
          that the dur attribute here is not a true duration, but
          rather a time stamp for the end point of the
          harmony.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.harm">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.visualoffset2.ho"/>
    <xs:attributeGroup ref="att.visualoffset2.to"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="extender" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates the presence of an extension
          symbol, typically a dash or underscore, drawn from the end
          of the harmonic indication to the point indicated by the dur
          attribute.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rendgrid">
      <xs:annotation>
        <xs:documentation>when set to 'grid', the chord tablature grid
          defined in the chordtable should be rendered instead of the
          text contents of the &lt;harm&gt; element; if
          rendgrid equals 'gridname', both the &lt;harm&gt;
          element's text and the grid should be displayed; otherwise,
          only the 'name' of the chord, i.e., the text content of the
          &lt;harm&gt; element, e.g. 'Cmaj' or 'V7/III',
          should be rendered.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="grid"/>
          <xs:enumeration value="gridname"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for harppedal                                   * -->
  <xs:attributeGroup name="att.anl.harppedal">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.harppedal">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.harppedal">
    <xs:annotation>
      <xs:documentation>Logical domain attributes. The pedal setting,
        i.e., flat, natural, or sharp, for each diatonic pitch name is
        indicated by the seven letter-named
        attributes.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="c" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the C
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="d" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the D
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="e" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the E
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="f" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the F
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="g" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the G
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="a" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the A
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="b" default="n">
      <xs:annotation>
        <xs:documentation>indicates the setting for the B
          strings.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="s"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.harppedal">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for lyrics                                      * -->
  <xs:attributeGroup name="att.anl.lyrics">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.lyrics">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="layer" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the layer to which the lyrics
          apply.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="staff" use="required"
      type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>signifies the staff to which the lyrics
          apply.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.lyrics">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.typography"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for measure                                     * -->
  <xs:attributeGroup name="att.anl.measure">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.measure">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes. The tstamp.ges and
        tstamp.real attributes encode the onset time of the measure.
        In reality, this is usually the same as the onset time of the
        first event in the measure.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="tstamp.ges" type="data.INTEGERNONNEGATIVE">
      <xs:annotation>
        <xs:documentation>used to record the onset time in pulses per
          quarter note (ppq, MusicXML divisions, or MIDI clicks) since
          the start of the file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tstamp.real" type="data.ISOTIME">
      <xs:annotation>
        <xs:documentation>holds the onset time as a real-time offset,
          i.e., HH:MM:SS.ss, since the beginning of the
          file.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.measure">
    <xs:annotation>
      <xs:documentation>Logical domain attributes. The n attribute
        contains a name or number associated with the measure (Read,
        p. 445). Often, this is an integer, but not always. For
        example, some measures, especially incomplete measures or
        those under an ending mark, may have labels that contain an
        integer plus a suffix, such as '12a'. Measures may even have
        labels, especially in editorial or analytical uses of MEI,
        that are entirely non-numeric strings. Measure numbers may be
        machine-generated instead of encoding them in the markup.
        However, an explicit measure number should restart numbering
        with the given value.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="complete">
      <xs:annotation>
        <xs:documentation>allows the encoding of whether a measure
          matches the prevailing meter: a value of 'c' indicates a
          metrically complete measure, 'i' indicates a measure with
          not enough beats, while 'o' is for measures with too many
          beats.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="c"/>
          <xs:enumeration value="i"/>
          <xs:enumeration value="o"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="control" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether or not the right barline
          is "controlling", that is, indicates a point of alignment
          across all the parts. Barlines within a score are usually
          'controlling'; that is, they 'line up'. Barlines within a
          part may or may not be 'controlling' with respect to other
          parts.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="join" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>indicates a measure which metrically
          completes the current one.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="left" type="data.BARRENDITION">
      <xs:annotation>
        <xs:documentation>indicates the visual rendition of the left
          barline. It is present here only for facilitation of
          translation from legacy encodings which use it. Usually, it
          can be safely ignored.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="right" type="data.BARRENDITION">
      <xs:annotation>
        <xs:documentation>indicates the function of the right barline
          and is structurally important.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.measure">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="barplace" type="data.BARPLACE">
      <xs:annotation>
        <xs:documentation>describes the placement of the right
          barline.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="taktplace" type="data.STAFFLOC">
      <xs:annotation>
        <xs:documentation>If takt barlines are to be used, then the
          taktplace attribute may be used to denote the staff location
          of the shortened barline. The location may include staff
          lines, spaces, and the spaces directly above and below the
          staff. The value ranges between 0 (just below the staff) to
          2 * number of staff lines (directly above the staff). For
          example, on a 5-line staff the lines would be numbered
          1,3,5,7, and 9 while the spaces would be numbered
          0,2,4,6,8,10. For example, a value of '9' puts the barline
          through the top line of a 5-line staff.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="width" type="data.NUMBER">
      <xs:annotation>
        <xs:documentation>For rendering purposes, some music data
          systems rely on measure width data. The width attribute
          allows for the capture of this data in MEI for output back
          to these systems.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for mordent                                     * -->
  <xs:attributeGroup name="att.anl.mordent">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.mordent">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attribute name="accidupper" type="data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>records the written accidental associated
          with the upper neighboring note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="accidlower" type="data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>records the written accidental associated
          with the lower neighboring note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="form">
      <xs:annotation>
        <xs:documentation>'norm' signals the normal form, that is, the
          written note, followed by its lower neighbor, with a return
          to the written note, while 'inv' indicates the inverted
          form, that is, the principal note, followed by its upper
          neighbor, with a return to the principal note.
          Traditionally, the 'normal' mordent is written as a short
          wavy line with a vertical line through it and the inverted
          mordent is written without the vertical line. However, the
          meaning of these signs is sometimes reversed. See Read, p.
          245-246. Another attribute in the visual domain would be
          necessary in order to be completely explicit about which
          visual symbol is actually to be rendered.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="inv"/>
          <xs:enumeration value="norm"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="long" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>When the long attribute is set to 'yes', a
          double or long mordent, consisting of 5 notes, is
          indicated.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.mordent">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.color"/>
    <xs:attributeGroup ref="att.placement"/>
    <xs:attributeGroup ref="att.visualoffset"/>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for mrest                                       * -->
  <xs:attributeGroup name="att.anl.mrest">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.mrest">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.mrest">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="fermata" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>indicates the attachment of a fermata to
          this element. If visual information about the fermata needs
          to be recorded, then a &lt;fermata&gt; element
          should be employed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.mrest">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="cutout">
      <xs:annotation>
        <xs:documentation>When set to 'rest' the cutout attribute
          indicates that the staff lines are to be displayed but not
          the rest itself. When set to 'staff' even the staff lines
          should not be drawn for this measure.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="rest"/>
          <xs:enumeration value="staff"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="dur.vis" type="data.DURATION">
      <xs:annotation>
        <xs:documentation>records the durational symbol used in the
          source.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="size" type="data.SIZE">
      <xs:annotation>
        <xs:documentation>describes the relative size of the measure
          rest symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for mrpt                                        * -->
  <xs:attributeGroup name="att.anl.mrpt">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.mrpt">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.mrpt">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attribute name="expand" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether to render the source
          measure instead of the repeat symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for mrpt2                                       * -->
  <xs:attributeGroup name="att.anl.mrpt2">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.mrpt2">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.mrpt2">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.color"/>
    <xs:attribute name="expand" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>indicates whether to render the source
          measures instead of the repeat symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for mspace                                      * -->
  <xs:attributeGroup name="att.anl.mspace">
    <xs:annotation>
      <xs:documentation>Analytical domain attributes. Use the n
        attribute to explicitly encode this measure's position in a
        string of measures containing only &lt;mrest&gt;
        elements.</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.mspace">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.mspace">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="fermata" type="data.PLACE">
      <xs:annotation>
        <xs:documentation>used to indicate the attachment of a fermata
          to this element. If visual information about the fermata
          needs to be recorded, then a &lt;fermata&gt; element
          should be employed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.mspace">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attributeGroup ref="att.visualoffset"/>
    <xs:attributeGroup ref="att.xy"/>
    <xs:attribute name="cutout">
      <xs:annotation>
        <xs:documentation>When set to 'rest' the cutout attribute
          indicates that the staff lines are to be displayed but not
          the rest itself. When set to 'staff' even the staff lines
          should not be drawn for this measure.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="rest"/>
          <xs:enumeration value="staff"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="dur.vis" type="data.DURATION">
      <xs:annotation>
        <xs:documentation>used to record a non-standard durational
          symbol used in the source.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for multirest                                   * -->
  <xs:attributeGroup name="att.anl.multirest">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ges.multirest">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dur.ges">
      <xs:annotation>
        <xs:documentation>records performed duration information that
          differs from the written values. Its value may be expressed
          in one of several forms, such as measures[s]+
          beat[s].beatpart, ppq (MIDI clicks), Humdrum **recip values,
          or MusicXML 'divisions', etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="instr" type="xs:IDREF">
      <xs:annotation>
        <xs:documentation>provides a way of pointing to a MIDI
          instrument definition. It must contain the ID of an
          &lt;instrdef&gt; element elsewhere in the
          document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.log.multirest">
    <xs:annotation>
      <xs:documentation>Logical domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.event"/>
    <xs:attribute name="num" use="required"
      type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>indicates the number of measures of
          rest.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.vis.multirest">
    <xs:annotation>
      <xs:documentation>Visual domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.altsym"/>
    <xs:attribute name="block" type="data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>When the block attribute is used,
          combinations of the 1, 2, and 4 measure rest forms (Read, p.
          104) should be rendered instead of the modern form or the
          alternative symbol.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <!-- ************************************************************** -->
  <!-- * Attributes for multirpt                                    * -->
  <xs:attributeGroup name="att.anl.multirpt">
    <xs:annotation>
      <xs:documentation>Analytical domain
        attributes</xs:documentation>
    </xs:annotation>
    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
