    <xs:attributeGroup ref="att.log.midi"/>
    <xs:attributeGroup ref="att.anl.midi"/>
  </xs:attributeGroup>
  <xs:element name="noteoff">
    <xs:annotation>
      <xs:documentation>note-off  MIDI note-off
        event.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="attlist.noteoff"/>
