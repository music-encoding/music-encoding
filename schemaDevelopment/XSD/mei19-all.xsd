  <xs:attributeGroup name="att.ges.nota.base">
    <xs:annotation>
      <xs:documentation>Gestural domain attributes</xs:documentation>
    </xs:annotation>
    <xs:attribute name="num" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with numbase, describes duration as a
          ratio. Num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="numbase" type="data.INTEGERPOSITIVE">
      <xs:annotation>
        <xs:documentation>along with num, describes duration as a
          ratio. Num is the first value in the ratio, while numbase is
          the second.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="accid.ges" type="data.ACCIDENTAL.IMPLICIT">
      <xs:annotation>
        <xs:documentation>records the performed pitch inflection when
          it differs from the written accidental.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="artic.ges" type="data.ARTICULATIONS">
      <xs:annotation>
        <xs:documentation>records performed articulation that differs
          from the written value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="detune"/>
    <xs:attribute name="dur.ges">
      <xs:annotation>
