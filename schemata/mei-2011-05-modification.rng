  <define name="chord">
    <element name="chord">
      <a:documentation
        xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">chord  A
        simultaneous sounding of two or more notes in the same layer *with the
        same duration*.</a:documentation>
      <zeroOrMore>
        <choice>
          <ref name="note"/>
          <ref name="artic"/>
        </choice>
      </zeroOrMore>

      <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
        id="chord-constraint-Check_instr">
        <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
          xmlns:rng="http://relaxng.org/ns/structure/1.0"
          xmlns:sch="http://purl.oclc.org/dsdl/schematron"
          context="mei:chord[@instr]">
          <sch:assert
            test="every $i in tokenize(@instr, '\s+') satisfies                 $i=preceding::mei:instrdef[@xml:id]/@xml:id"
          />
        </sch:rule>
      </pattern>

      <ref name="att.common.attributes"/>
      <ref name="att.facsimile.attributes"/>
      <ref name="att.chord.log.attributes"/>
      <ref name="att.chord.vis.attributes"/>
      <ref name="att.chord.ges.attributes"/>
      <ref name="att.chord.anl.attributes"/>
