  <define name="att.staffdef.vis.attribute.lines.color">
    <optional>
      <attribute name="lines.color">
        <a:documentation
          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">captures
          the colors of the staff lines. The value is structured; that is, it
          should have the same number of space-separated RGB values as the
          number of lines indicated by the lines attribute. A line can be made
          invisible by assigning it the same RGB value as the background,
          usually white.</a:documentation>
        <ref name="data.COLORS"/>
      </attribute>
    </optional>
  </define>
  <define name="att.staffdef.vis.attribute.lines.visible">
    <optional>
