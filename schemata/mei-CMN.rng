  <define name="mei_data.ARTICULATION">
    <choice>
      <value>acc</value>
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Accent (Unicode
        1D17B).</a:documentation>
      <value>stacc</value>
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Staccato
        (Unicode 1D17C).</a:documentation>
      <value>ten</value>
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tenuto (Unicode
        1D17D).</a:documentation>
      <value>stacciss</value>
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Staccatissimo
        (Unicode 1D17E).</a:documentation>
      <value>marc</value>
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marcato
        (Unicode 1D17F).</a:documentation>
