      <zeroOrMore>
        <choice>
          <text/>
          <ref name="mei_model.textPhraseLike.limited"/>
        </choice>
      </zeroOrMore>
      <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
        id="mei-contributor-check_contributor_role-constraint-121">
        <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
          xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0"
          xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="mei:contributor">
          <sch:assert
            test="not(matches(normalize-space(lower-case(@role)), 
