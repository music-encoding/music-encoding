    <xs:attributeGroup ref="mei:mei_att.visualoffset2.to.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.xy.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attributes">
