  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.multinummeasures.attribute.multi.number">
    <xs:attribute name="multi.number" type="mei:mei_data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>Indicates whether programmatically calculated counts of multiple measures
          of rest (mRest) and whole measure repeats (mRpt) in parts should be
          rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
