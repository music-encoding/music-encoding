      <xs:group ref="mei:mei_model.midiLike"/>
    </xs:choice>
  </xs:group>
  <xs:element name="app" substitutionGroup="mei:mei_model.appLike">
    <xs:annotation>
      <xs:documentation>(apparatus)  Contains one or more alternative encodings.</xs:documentation>
    </xs:annotation>
  </xs:element>
