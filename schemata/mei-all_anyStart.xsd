    <xs:attribute name="barthru" type="mei:mei_data.BOOLEAN">
      <xs:annotation>
        <xs:documentation>Indicates whether bar lines go across the space between staves (true) or
          are only drawn across the lines of each staff (false).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.staffident.attributes">
