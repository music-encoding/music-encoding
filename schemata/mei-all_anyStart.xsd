  <xs:attributeGroup name="mei_att.spacing.attribute.spacing.packexp">
    <xs:attribute name="spacing.packexp" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>Describes a note's spacing relative to its time value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.spacing.attribute.spacing.packfact">
    <xs:attribute name="spacing.packfact" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>Describes the note spacing of output.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.spacing.attribute.spacing.staff">
    <xs:attribute name="spacing.staff" type="mei:mei_data.MEASUREMENTREL">
      <xs:annotation>
        <xs:documentation>Specifies the minimum amount of space between adjacent staves in the same
          system; measured from the bottom line of the staff above to the top line of the staff
          below.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.spacing.attribute.spacing.system">
    <xs:attribute name="spacing.system">
      <xs:annotation>
        <xs:documentation>Describes the space between adjacent systems; a pair of space-separated
          values (minimum and maximum, respectively) provides a range between which a rendering
          system-supplied value may fall, while a single value indicates a fixed amount of space;
          that is, the minimum and maximum values are equal.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list>
              <xs:simpleType>
                <xs:union memberTypes="mei:mei_data.MEASUREMENTREL mei:mei_data.MEASUREMENTREL"/>
              </xs:simpleType>
            </xs:list>
          </xs:simpleType>
          <xs:minLength value="1"/>
          <xs:maxLength value="2"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
