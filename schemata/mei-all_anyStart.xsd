      <xs:element ref="mei:fig"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="mei_model.figureLike_sequenceOptional">
    <xs:sequence>
      <xs:element minOccurs="0" ref="mei:fig"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="mei_model.figureLike_sequenceOptionalRepeatable">
