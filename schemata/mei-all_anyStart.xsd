  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.size">
    <xs:attribute name="text.size" type="mei:mei_data.FONTSIZE">
      <xs:annotation>
        <xs:documentation>Provides a default value for the font size of text (other than lyrics)
          when this information is not provided on the individual elements.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.textstyle.attribute.text.style">
