    <xs:attributeGroup ref="mei:mei_att.color.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.extsym.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.placement.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.typography.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.visualoffset.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.ornam.attributes">
    <xs:attributeGroup ref="mei:mei_att.ornam.attribute.ornam"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.ornam.attribute.ornam">
    <xs:attribute name="ornam">
      <xs:annotation>
        <xs:documentation>Indicates that this element has an attached ornament. If visual
          information about the ornament is needed, then one of the elements that represents an
          ornament (mordent, trill, or turn) should be employed.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list itemType="mei:mei_data.ORNAM.cmn"/>
          </xs:simpleType>
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.ornamentaccid.attributes">
    <xs:attributeGroup ref="mei:mei_att.ornamentaccid.attribute.accidupper"/>
    <xs:attributeGroup ref="mei:mei_att.ornamentaccid.attribute.accidlower"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.ornamentaccid.attribute.accidupper">
    <xs:attribute name="accidupper" type="mei:mei_data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>Records the written accidental associated with an upper neighboring
          note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.ornamentaccid.attribute.accidlower">
    <xs:attribute name="accidlower" type="mei:mei_data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>Records the written accidental associated with a lower neighboring
          note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.trill.anl.attributes">
    <xs:attributeGroup ref="mei:mei_att.common.anl.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.trill.ges.attributes">
    <xs:attributeGroup ref="mei:mei_att.duration.performed.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.trill.log.attributes">
    <xs:attributeGroup ref="mei:mei_att.controlevent.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.augmentdots.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.duration.additive.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.ornamentaccid.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.startendid.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.timestamp2.musical.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.trill.vis.attributes">
    <xs:attributeGroup ref="mei:mei_att.altsym.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.color.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.extender.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.placement.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.extsym.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.typography.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.visualoffset.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.visualoffset2.ho.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.visualoffset2.to.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.xy.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.turn.anl.attributes">
    <xs:attributeGroup ref="mei:mei_att.common.anl.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.turn.log.attributes">
    <xs:attributeGroup ref="mei:mei_att.controlevent.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.ornamentaccid.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.startid.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.turn.log.attribute.delayed"/>
    <xs:attributeGroup ref="mei:mei_att.turn.log.attribute.form"/>
  </xs:attributeGroup>
