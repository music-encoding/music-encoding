    <xs:attribute name="trans.semi" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>Records the amount of pitch shift in semitones, e.g., C to C = 1, C to D
          = 1, necessary to calculate the sounded pitch from the written one.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
