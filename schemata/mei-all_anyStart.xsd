  <xs:attributeGroup name="mei_att.beaming.vis.attribute.beam.rend">
    <xs:attribute name="beam.rend">
      <xs:annotation>
        <xs:documentation>Encodes whether a beam is "feathered" and in which
          direction.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="acc">
            <xs:annotation>
              <xs:documentation>Beam lines grow farther apart from left to right.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="rit">
            <xs:annotation>
              <xs:documentation>Beam lines grow closer together from left to
                right.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="norm">
            <xs:annotation>
              <xs:documentation>Beam lines are equally-spaced over the entire length of the
                beam.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
