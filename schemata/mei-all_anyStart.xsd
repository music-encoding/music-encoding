  <xs:attributeGroup name="mei_att.scoreDef.vis.attribute.vu.height">
    <xs:attribute name="vu.height">
      <xs:annotation>
        <xs:documentation>Defines the height of a "virtual unit" (vu) in terms of real-world units.
          A single vu is half the distance between the vertical center point of a staff line and
          that of an adjacent staff line.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:pattern value="\d+(\.\d+)?(cm|mm|in|pt|pc)"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
