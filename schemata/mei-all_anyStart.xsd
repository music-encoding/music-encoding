    <xs:attributeGroup ref="mei:mei_att.handident.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.responsibility.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.sequence.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.source.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.crit.attribute.cause"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.crit.attribute.cause">
    <xs:attribute name="cause" type="xs:NMTOKEN">
      <xs:annotation>
        <xs:documentation>Classifies the cause for the variant reading, according to any appropriate
          typology of possible origins.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.rdg.anl.attributes">
