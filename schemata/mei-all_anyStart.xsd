      <xs:annotation>
        <xs:documentation>Signifies the staff on which a notated event occurs or to which a control
          event applies. Mandatory when applicable.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list itemType="xs:positiveInteger"/>
          </xs:simpleType>
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
