      <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.coordinated.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.datapointing.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.declaring.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.startid.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.typed.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="zone">
    <xs:annotation>
      <xs:documentation>Defines an area of interest within a surface or graphic
        file.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mei:mei_model.figDescLike"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mei:mei_model.graphicLike"/>
      </xs:sequence>
      <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.coordinated.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.datapointing.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.typed.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="mei_att.tabular.attributes">
    <xs:attributeGroup ref="mei:mei_att.tabular.attribute.colspan"/>
    <xs:attributeGroup ref="mei:mei_att.tabular.attribute.rowspan"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.tabular.attribute.colspan">
    <xs:attribute name="colspan" type="xs:positiveInteger">
      <xs:annotation>
        <xs:documentation>The number of columns spanned by this cell.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.tabular.attribute.rowspan">
    <xs:attribute name="rowspan" type="xs:positiveInteger">
