          <xs:element ref="mei:mei_model.divLike"/>
          <xs:group ref="mei:mei_model.milestoneLike.music"/>
          <xs:element ref="mei:mei_model.staffGrpLike"/>
          <xs:group ref="mei:mei_model.annotLike"/>
          <xs:element ref="mei:mei_model.graphicprimitiveLike"/>
          <xs:element ref="mei:mei_model.editLike"/>
          <xs:element ref="mei:mei_model.transcriptionLike"/>
          <xs:group ref="mei:mei_model.rdgPart.critapp"/>
          <xs:group ref="mei:mei_model.sectionPart"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.crit.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.pointing.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.rdg.anl.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.targeteval.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.typed.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="mei_att.agentident.attributes">
    <xs:attributeGroup ref="mei:mei_att.agentident.attribute.agent"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.agentident.attribute.agent">
    <xs:attribute name="agent" type="xs:string">
      <xs:annotation>
        <xs:documentation>Signifies the causative agent of damage, illegibility, or other loss of
          original text.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.cpMark.anl.attributes">
    <xs:attributeGroup ref="mei:mei_att.common.anl.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.cpMark.ges.attributes">
    <xs:attributeGroup ref="mei:mei_att.duration.performed.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="mei_att.cpMark.log.attributes">
    <xs:attributeGroup ref="mei:mei_att.controlevent.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.startendid.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.timestamp2.musical.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.origin.timestamp.musical.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.origin.staffident.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.origin.layerident.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.origin.startendid.attributes"/>
    <xs:attributeGroup ref="mei:mei_att.octavedisplacement.attributes"/>
