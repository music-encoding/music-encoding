      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mei:surface"/>
      </xs:sequence>
      <xs:attributeGroup ref="mei:mei_att.common.attributes"/>
      <xs:attributeGroup ref="mei:mei_att.declaring.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="surface">
    <xs:annotation>
      <xs:documentation>Defines a writing surface in terms of a rectangular coordinate space,
        optionally grouping one or more graphic representations of that space, and rectangular zones
        of interest within it.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mei:mei_model.figDescLike"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mei:mei_model.graphicLike"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="mei:zone"/>
