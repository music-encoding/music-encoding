         </pattern>
         <ref name="mei_att.common.attributes"/>
         <ref name="mei_att.caesura.anl.attributes"/>
         <ref name="mei_att.caesura.ges.attributes"/>
         <ref name="mei_att.caesura.log.attributes"/>
         <ref name="mei_att.caesura.vis.attributes"/>
         <empty/>
      </element>
   </define>
