   <define name="mei_data.FONTSIZENUMERIC">
      <data type="token">
         <param name="pattern">\d*(\.\d+)?(pt|vu)</param>
         <except>
            <choice>
               <data type="token">
                  <param name="pattern">(pt|vu)</param>
