   <define name="mei_argument">
      <element name="argument">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Contains a formal list or prose description of topics addressed.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="mei_model.headLike"/>
               <ref name="mei_model.milestoneLike.text"/>
