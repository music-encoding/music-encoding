   <define name="mei_epigraph">
      <element name="epigraph">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Contains a quotation, anonymous or attributed, appearing on a title page.</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="mei_model.paracontentPart"/>
