    <xs:annotation>
      <xs:documentation>Used by scoredef and staffdef to provide
      default description of piano pedal rendition</xs:documentation>
    </xs:annotation>
    <xs:attribute name="pedal.style">
      <xs:annotation>
        <xs:documentation>determines whether piano pedal marks
          should be drawn as lines or as terms (e.g. 'Ped.' for pedal down and
          '*' for pedal up).</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="line"/>
          <xs:enumeration value="pedstar"/>
          <xs:enumeration value="altpedstar"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
