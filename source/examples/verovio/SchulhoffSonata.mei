          </useRestrict>
        </availability>
      </pubStmt>
      <seriesStmt>
        <title>MEI Guidelines examples</title>
      </seriesStmt>
    </fileDesc>
  </meiHead>
  <music>
    <body>
      <mdiv n="1" label="Schulhoff's Violin Sonata" xml:id="Schulhoff">
        <score>
          <scoreDef system.leftline="false">
            <staffGrp>
              <staffDef n="1" clef.line="2" clef.shape="G" lines="5" />
            </staffGrp>
          </scoreDef>
          <section>
            <measure right="single">
              <staff n="1">
                <layer>
                  <beam>
                    <note dur="8" oct="4" pname="a">
                      <accid accid="n" />
                      <artic artic="stacc acc" />
                    </note>
                    <note dur="8" oct="4" pname="b">
                      <artic artic="stacc acc" />
                    </note>
                    <note dur="8" oct="5" pname="c">
                      <accid accid="n" />
                      <artic artic="stacc acc" />
                    </note>
                    <note dur="8" oct="4" pname="d">
                      <accid accid="s" />
                      <artic artic="stacc acc" />
                    </note>
                  </beam>
                  <note dur="8" oct="4" pname="e" xml:id="note1">
                    <artic artic="stacc acc" />
                  </note>
                  <note dur="8" oct="4" pname="f" xml:id="note2">
                    <accid accid="s" />
                    <artic artic="stacc acc" />
                  </note>
                </layer>
              </staff>
              <?edit-start?>
              <beamSpan startid="#note1" endid="#note4" plist="#note1 #note2 #note3 #note4" />
              <?edit-end?>
            </measure>
            <measure right="single">
              <staff n="1">
                <layer>
                  <note dur="8" oct="4" pname="g" xml:id="note3">
                    <accid accid="n" />
                    <artic artic="stacc acc" />
                  </note>
                  <note dur="8" oct="3" pname="a" xml:id="note4">
                    <accid accid="n" />
                    <artic artic="stacc acc" />
                  </note>
                  <beam>
                    <note dur="8" oct="3" pname="b">
                      <accid accid="f" />
                      <artic artic="stacc acc" />
                    </note>
                    <note dur="8" oct="4" pname="c">
                      <accid accid="n" />
                      <artic artic="stacc acc" />
                    </note>
                  </beam>
                  <note dur="4" oct="4" pname="d">
                    <accid accid="f" />
                    <artic artic="ten acc" />
                  </note>
                </layer>
              </staff>
            </measure>
          </section>
        </score>
      </mdiv>
    </body>
  </music>
</mei>
