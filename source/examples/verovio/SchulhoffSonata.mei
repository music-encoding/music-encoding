<?xml-model href="https://music-encoding.org/schema/dev/mei-CMN.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="5.1-dev">
  <meiHead>
    <fileDesc>
      <titleStmt>
        <title>Schulhoff's Violin Sonata (excerpt)</title>
        <editor>
          <persName>Klaus Rettinghaus</persName>
        </editor>
      </titleStmt>
      <pubStmt>
        <date>2024</date>
        <availability>
          <useRestrict label="licence" auth="https://creativecommons.org/licenses/by/" codedval="4.0">
