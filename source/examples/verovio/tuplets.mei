      <mdiv n="2" label="hierarchy-crossing">
        <score>
          <scoreDef system.leftline="false">
            <staffGrp>
              <staffDef clef.shape="G" clef.line="2" meter.count="2" meter.unit="4" n="1" lines="5" />
            </staffGrp>
          </scoreDef>
          <section>
            <measure>
              <?edit-start?>
              <staff n="1">
                <layer>
                  <rest dur="8" xml:id="first-tuplet-start" />
                  <beam>
                    <note stem.dir="up" dur="8" pname="c" oct="4" xml:id="first-tuplet-art" />
                    <note stem.dir="up" dur="8" pname="d" oct="4" xml:id="first-tuplet-end" />
                    <note stem.dir="up" dur="8" pname="e" oct="4" xml:id="second-tuplet-start" />
                    <note stem.dir="up" dur="8" pname="f" oct="4" xml:id="second-tuplet-part" />
                    <note stem.dir="up" dur="8" pname="g" oct="4" xml:id="second-tuplet-end" />
                  </beam>
                </layer>
              </staff>
              <tupletSpan num="3" numbase="2" startid="#first-tuplet-start" endid="#first-tuplet-end" />
              <tupletSpan num="3" numbase="2" startid="#second-tuplet-start" endid="#second-tuplet-end" />
              <?edit-end?>
            </measure>
            <measure>
              <staff n="1">
                <layer>
                  <?edit-start?>
                  <rest dur="8" xml:id="unbeamed" />
                  <note stem.dir="up" dur="8" pname="f" oct="4" xml:id="beamed01" />
                  <note stem.dir="up" dur="8" pname="g" oct="4" xml:id="beamed02" />
                  <note stem.dir="up" dur="8" pname="a" oct="4" xml:id="beamed03" />
                  <note stem.dir="up" dur="8" pname="b" oct="4" xml:id="beamed04">
                    <accid accid="f" />
                  </note>
                  <note stem.dir="up" dur="8" pname="c" oct="5" xml:id="beamed05" />
                  <?edit-end?>
                </layer>
              </staff>
              <beamSpan startid="#beamed01" endid="#beamed05" />
              <tupletSpan num="3" numbase="2" startid="#unbeamed" endid="#beamed02" />
              <tupletSpan num="3" numbase="2" startid="#beamed03" endid="#beamed05" />
            </measure>
