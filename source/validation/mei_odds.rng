            </zeroOrMore>
      
            <zeroOrMore>
               <group>
        
                  <ref name="model.divBottom"/>
        
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="mei_odds-lg-atleast1oflggapl-constraint-assert-8">
