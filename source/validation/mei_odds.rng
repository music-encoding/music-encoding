            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="aloud"
                       a:defaultValue="unknown">
               <a:documentation>may be used to indicate whether the quoted matter is regarded as having been vocalized or signed.</a:documentation>
               <choice>
                  <data type="boolean"/>
                  <choice>
                     <value>unknown</value>
                     <a:documentation/>
                     <value>inapplicable</value>
                     <a:documentation/>
                  </choice>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="direct"
                       a:defaultValue="true">
               <a:documentation>may be used to indicate whether the quoted matter is regarded as direct or indirect speech.</a:documentation>
               <choice>
                  <data type="boolean"/>
                  <choice>
                     <value>unknown</value>
                     <a:documentation/>
                     <value>inapplicable</value>
                     <a:documentation/>
                  </choice>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="quote">
