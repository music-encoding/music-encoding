<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../schemata/mei-all_anyStart.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="../../schemata/mei-all_anyStart.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>

<!-- Stone, Music Notation in the Twentieth Century, p. 194, ex. 1, "triple stop" -->
<!-- Fingering diagram for the flute. The default behavior (without @orient) is to stack the
  individual <fing> elements. So, @orient is redundant in this case. Setting @orient to "horiz"
  would implement Gould's recommendation to provide flute fingering diagrams in a horizontal
  fashion. In that case, @ho would have to be interpreted as as though it were @vo or actually
  replaced by @vo. -->
<!-- @staff has not been encoded for ease of parsing, but it should be present. -->
<fingGrp xmlns="http://www.music-encoding.org/ns/mei"
  xmlns:mei="http://www.music-encoding.org/ns/mei" xmlns:xlink="http://www.w3.org/1999/xlink"
  tstamp="1" orient="vert">
  <fing>
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole"/>
  </fing>
  <fing>
    <!-- The side key is rendered slightly to the left. -->
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole" ho="-1mm"/>
  </fing>
  <fing>
    <symbol glyph.num="#xE5F9" glyph.name="windOpenHole"/>
  </fing>
  <fing>
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole"/>
  </fing>
  <fing>
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole"/>
  </fing>
  <fing>
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole"/>
  </fing>
  <fing>
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole"/>
  </fing>
  <fing>
    <!-- The foot is rendered slightly to the left. -->
    <symbol glyph.num="#xE5F4" glyph.name="windClosedHole" ho="-1mm"/>||</fing>
</fingGrp>
