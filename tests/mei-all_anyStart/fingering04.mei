<?xml-model href="../../schemata/mei-all_anyStart.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="../../schemata/mei-all_anyStart.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>

<!-- Stone, Music Notation in the Twentieth Century, p. 194, ex. 2, "triple stop" -->
<!-- Fingering diagram for the oboe. The outer fingGrp is rendered vertically, while the 
  inner two fingGrp elements are rendered horizontally in order to deal with the side keys. -->
<!-- @staff has not been encoded for ease of parsing, but it should be present. -->
<fingGrp xmlns="http://www.music-encoding.org/ns/mei"
  xmlns:mei="http://www.music-encoding.org/ns/mei" xmlns:xlink="http://www.w3.org/1999/xlink"
  tstamp="1">
  <fing>
    <symbol glyphnum="#xE5F7" glyphname="windHalfClosedHole2"/>
  </fing>
  <fing>
    <symbol glyphnum="#xE5F4" glyphname="windClosedHole"/>
  </fing>
  <fingGrp orient="horiz">
    <fing>G#</fing>
    <fing><symbol glyphnum="#xE5F4" glyphname="windClosedHole"/></fing>
    <fing>Bb</fing>
  </fingGrp>
  <fing>
    <symbol glyphnum="#xE5F9" glyphname="windOpenHole"/>
  </fing>
  <fing>
    <symbol glyphnum="#xE5F4" glyphname="windClosedHole"/>
  </fing>
  <fingGrp orient="horiz">
    <fing>Eb</fing>
    <fing><symbol glyphnum="#xE5F4" glyphname="windClosedHole"/></fing>
  </fingGrp>
</fingGrp>
